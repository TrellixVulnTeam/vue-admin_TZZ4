e=\"handleCurrentChange\"\r\n        />\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n    name: 'Pagination',\r\n    props: {\r\n        page: {\r\n            type: Number,\r\n            default: 1,\r\n            required: true\r\n        },\r\n        limit: {\r\n            type: Number,\r\n            default: 10,\r\n            required: true\r\n        },\r\n        background: {\r\n            type: Boolean,\r\n            default: true\r\n        },\r\n        pageSizes: {\r\n            type: Array,\r\n            default: () => [10, 15, 20, 50]\r\n        },\r\n        total: {\r\n            type: Number,\r\n            default: 0,\r\n            required: true\r\n        },\r\n        layout: {\r\n            type: String,\r\n            default: 'total, prev, pager, next, sizes, jumper'\r\n        }\r\n    },\r\n    computed: {\r\n        currentPage: {\r\n            get() {\r\n                return this.page\r\n            },\r\n            set(val) {\r\n                this.$emit('update:page', val)\r\n            }\r\n        },\r\n        pageSize: {\r\n            get() {\r\n                return this.limit\r\n            },\r\n            set(val) {\r\n                this.emit('update:limit', val)\r\n            }\r\n        }\r\n    },\r\n    methods: {\r\n        handleSizeChange(val) {\r\n            this.$emit('pagination', {page: this.currentPage, limit: val})\r\n        },\r\n        handleCurrentChange(val) {\r\n            this.$emit('pagination', {page: val, limit: this.pageSize})\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\">\r\n.pagination-wrapper {\r\n    margin-top: 25px;\r\n\r\n    .el-pagination {\r\n        float: right;\r\n    }\r\n}\r\n</style>\r\n"]}]}