  <el-form-item label=\"vip到期时间\" prop=\"vipEndAt\">\r\n                    <el-date-picker type=\"date\" placeholder=\"vip到期时间\" v-model=\"form.vipEndAt\" style=\"width: 100%;\"></el-date-picker>\r\n                </el-form-item>\r\n\r\n                <el-form-item class=\"submit-box\">\r\n                    <el-button type=\"primary\" @click=\"submitForm('form')\" style=\"margin-right: 50px\">提&nbsp;&nbsp;&nbsp;交</el-button>\r\n                    <el-button @click=\"resetForm('form')\">重&nbsp;&nbsp;&nbsp;置</el-button>\r\n                </el-form-item>\r\n            </el-form>\r\n        </div>\r\n    </el-dialog>\r\n</template>\r\n\r\n<script>\r\nimport { banLevel } from '@/dict/index'\r\n\r\nexport default {\r\n    components: { banLevel },\r\n    data() {\r\n        return {\r\n            form: {\r\n                uid: '',\r\n                nickname: '',\r\n                vipEndAt: ''\r\n            },\r\n            banLevel,\r\n            dialogVisible: false,\r\n            rules: {\r\n                vipEndAt: [\r\n                    {required: true, message: '不能为空', trigger: 'blur'}\r\n                ]\r\n            }\r\n        }\r\n    },\r\n    methods: {\r\n        init(row){\r\n            this.form.id = row.id\r\n            this.form.nickname = row.nickname\r\n        },\r\n        submitForm(formName) {\r\n            const $this = this\r\n            this.$refs[formName].validate((valid) => {\r\n                if (valid) {\r\n                    this.$service.user.updateUser(this.form, function (result){\r\n                        if (result) {\r\n                            $this.$message.success(\"保存成功!\")\r\n                            $this.closeDialog()\r\n                        } else {\r\n                            $this.$message.error(\"保存失败!\")\r\n                        }\r\n                    })\r\n                }\r\n            })\r\n        },\r\n        closeDialog() {\r\n            this.dialogVisible = false\r\n            this.$emit('fetchData');\r\n        },\r\n        resetForm(formName) {\r\n            this.$refs[formName].resetFields()\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\">\r\n.form-list-wrapper {\r\n    .el-card {\r\n    }\r\n\r\n    .form-list {\r\n        width: 80%;\r\n        margin: 0 auto;\r\n\r\n        .el-rate {\r\n            line-height: 2;\r\n        }\r\n\r\n        > .el-form-item {\r\n            margin-bottom: 22px;\r\n\r\n            .line {\r\n                text-align: center;\r\n            }\r\n\r\n            .tip-title {\r\n                text-align: right;\r\n                color: #606266;\r\n            }\r\n        }\r\n\r\n        .submit-box {\r\n            margin-top: 35px;\r\n            margin-left: 0;\r\n            text-align: center;\r\n        }\r\n    }\r\n}\r\n</style>\r\n"]}]}