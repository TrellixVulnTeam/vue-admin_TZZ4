}</span>\n                </li>\n                <li>\n                    <label>权限区域：</label>\n                    <span>{{ userInfo.areaIds }}</span>\n                </li>\n                <li>\n                    <label>权限应用：</label>\n                    <span>{{ userInfo.appIds }}</span>\n                </li>\n                <li>\n                    <label>权限模块：</label>\n                    <span>{{ userInfo.modules }}</span>\n                </li>\n                <li>\n                    <label>备注：</label>\n                    <span>{{ userInfo.note }}</span>\n                </li>\n                <li>\n                    <label>登陆时间：</label>\n                    <span>{{ userInfo.loginAt }}</span>\n                </li>\n                <li>\n                    <label>登陆IP：</label>\n                    <span>{{ userInfo.loginIp }}</span>\n                </li>\n                <li>\n                    <label>创建时间：</label>\n                    <span>{{ userInfo.createdAt }}</span>\n                </li>\n            </ul>\n        </el-card>\n    </div>\n</template>\n\n<script>\n\nimport {toTime} from \"@/utils/date\";\n\nexport default {\n    name: 'UserCenter',\n    inject:[\"reload\"],\n    data() {\n        return {\n            userInfo: {\n                email: undefined\n            }\n        }\n    },\n    created() {\n        const $this = this\n        this.$service.user.getAdminInfo({}, function (result){\n            $this.userInfo.email = result.getEmail()\n            $this.userInfo.name = result.getName()\n            $this.userInfo.note = result.getNote()\n            $this.userInfo.areaIds = result.getAreaId()\n            $this.userInfo.appIds = result.getAppIdsList()\n            $this.userInfo.modules = result.getModules()\n            $this.userInfo.loginAt = toTime(result.getLoginAt()),\n            $this.userInfo.loginIp = result.getLoginIp()\n            $this.userInfo.createdAt = toTime(result.getCreatedAt())\n        })\n        /*getUserInfo().then(res => {\n            this.userInfo = res\n        })*/\n    }\n}\n</script>\n\n<style lang=\"less\">\n.user-center-wrapper {\n    .user-avatar {\n        float: left;\n        width: 150px;\n        height: 150px;\n    }\n\n    .user-info {\n        float: left;\n        width: 800px;\n        margin-left: 50px;\n        margin-bottom: 50px;\n\n        li {\n            height: 34px;\n            line-height: 34px;\n\n            label,\n            span {\n                display: inline-block;\n                vertical-align: middle;\n            }\n\n            label {\n                width: 80px;\n                margin-right: 12px;\n                text-align: right;\n            }\n        }\n    }\n}\n</style>\n"]}]}