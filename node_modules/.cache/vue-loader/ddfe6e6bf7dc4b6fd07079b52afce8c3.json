m-item label=\"结算时间\">\n                <el-date-picker v-model=\"search.settleAt\" type=\"date\" placeholder=\"选择日期\">\n                </el-date-picker>\n            </el-form-item>\n            <el-form-item label=\"最小余额\">\n                <el-input-number v-model=\"search.minBalance\" min=\"1\"/>\n            </el-form-item>\n        </el-form>\n        <span slot=\"footer\" class=\"dialog-footer\">\n            <el-button @click=\"closeDialog\">取 消</el-button>\n            <el-button type=\"primary\" @click=\"onSearch\">确 定</el-button>\n        </span>\n    </el-dialog>\n</template>\n\n<script>\n\nimport {getAreaList, getArrName} from \"@/utils/common\";\n\nexport default {\n    data() {\n        return {\n            dialogVisible: false,\n            search: {\n                areaId: 1,\n                settleAt: new Date(),\n                minBalance: 100\n            },\n            areaList: getAreaList()\n        }\n    },\n    methods: {\n        init(row){\n\n        },\n        onSearch() {\n            let $this = this\n            this.$service.settle.createSettleRecord(this.search, function (result){\n                if(result){\n                    $this.$message.success(\"预结算生成成功!\")\n                    this.$emit('createSettleRecord', {areaId: this.search.areaId});\n                    this.dialogVisible = false\n                    this.resetForm()\n                }else{\n                    $this.$message.error(\"预结算生成失败!\")\n                }\n            });\n\n\n\n        },\n        closeDialog() {\n            this.dialogVisible = false\n            this.resetForm()\n        }\n    }\n}\n</script>\n\n"]}]}