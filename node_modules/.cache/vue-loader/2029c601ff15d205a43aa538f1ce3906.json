"item in areaData\"\r\n                                   :key=\"item.value\"\r\n                                   :label=\"item.label\"\r\n                                   :value=\"item.value\">\r\n                        </el-option>\r\n                    </el-select>\r\n                </el-form-item>\r\n                <el-form-item label=\"账户名：\" prop=\"name\">\r\n                    <el-input v-model=\"form.name\" placeholder=\"请输入\" />\r\n                </el-form-item>\r\n                <el-form-item label=\"密码：\" prop=\"password\">\r\n                    <el-input v-model=\"form.password\" placeholder=\"请输入\" />\r\n                </el-form-item>\r\n                <el-form-item label=\"真实姓名：\" prop=\"url\">\r\n                    <el-input v-model=\"form.relname\" placeholder=\"请输入\" />\r\n                </el-form-item>\r\n                <el-form-item label=\"权限设置：\" prop=\"url\">\r\n                        <div class=\"content-box\" style=\"height: 360px;\">\r\n                            <el-transfer v-model=\"value1\"\r\n                                         :data=\"data\"\r\n                                         :titles=\"['权限', '已有权限']\"/>\r\n                        </div>\r\n                </el-form-item>\r\n                <el-form-item label=\"是否开启\" prop=\"isOpen\">\r\n                    <el-switch v-model=\"form.isOpen\"/>\r\n                </el-form-item>\r\n\r\n                <el-form-item class=\"submit-box\">\r\n                    <el-button type=\"primary\" @click=\"submitForm('form')\" style=\"margin-right: 50px\">确&nbsp;&nbsp;&nbsp;定</el-button>\r\n                    <el-button @click=\"resetForm('form')\">重&nbsp;&nbsp;&nbsp;置</el-button>\r\n                </el-form-item>\r\n            </el-form>\r\n        </div>\r\n    </el-dialog>\r\n</template>\r\n\r\n<script>\r\n\r\nexport default {\r\n    data() {\r\n        const baseData = () => {\r\n            const data = []\r\n            for (let i = 1; i < 10; i++) {\r\n                data.push({\r\n                    key: i,\r\n                    label: `备选项 ${i}`\r\n                })\r\n            }\r\n            return data\r\n        }\r\n        return {\r\n            data: baseData(),\r\n            form: {\r\n                type: '',\r\n            },\r\n            value1: [1, 5],\r\n            value2: [1, 5],\r\n            dialogVisible: false\r\n        }\r\n    },\r\n    methods: {\r\n        init(row){\r\n            this.form.type = row.type\r\n\r\n        },\r\n        submitForm(formName) {\r\n            this.$refs[formName].validate((valid) => {\r\n                if (valid) {\r\n                    // 此处添加后端接口\r\n                    alert('提交成功!')\r\n                } else {\r\n                    console.log('提交失败!')\r\n                    return false\r\n                }\r\n            })\r\n        },\r\n        handleChange(value, direction, movedKeys) {\r\n            console.log(value, direction, movedKeys)\r\n        },\r\n        resetForm(formName) {\r\n            this.$refs[formName].resetFields()\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\">\r\n.form-list-wrapper {\r\n    .el-card {\r\n    }\r\n\r\n    .form-list {\r\n        width: 80%;\r\n        margin: 0 auto;\r\n\r\n        .el-rate {\r\n            line-height: 2;\r\n        }\r\n\r\n        > .el-form-item {\r\n            margin-bottom: 22px;\r\n\r\n            .line {\r\n                text-align: center;\r\n            }\r\n\r\n            .tip-title {\r\n                text-align: right;\r\n                color: #606266;\r\n            }\r\n        }\r\n\r\n        .submit-box {\r\n            margin-top: 35px;\r\n            margin-left: 0;\r\n            text-align: center;\r\n        }\r\n    }\r\n}\r\n</style>\r\n"]}]}