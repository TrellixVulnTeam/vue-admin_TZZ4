AAA,IAAA,EAAA,KAAA,EAAA;AACA,cAAA,IAAA,GAAA;AACA,kBAAA,IAAA,CAAA,KAAA,EADA;AAEA,sBAAA,IAAA,CAAA,SAAA,EAFA;AAGA,qBAAA,IAAA,CAAA,QAAA,EAHA;AAIA,wBAAA,IAAA,CAAA,WAAA,EAJA;AAKA,oBAAA,IAAA,CAAA,OAAA,EALA;AAMA,oBAAA,IAAA,CAAA,OAAA,EANA;AAOA,wBAAA,IAAA,CAAA,cAAA,EAPA;AAQA,uBAAA,IAAA,CAAA,aAAA,EARA;AASA,uBAAA,IAAA,CAAA,cAAA,EATA;AAUA,wBAAA,IAAA,CAAA,UAAA,EAVA;AAWA,wBAAA,IAAA,CAAA,UAAA,EAXA;AAYA,0BAAA,IAAA,CAAA,YAAA,EAZA;AAaA,4BAAA,IAAA,CAAA,cAAA,EAbA;AAcA,0BAAA,IAAA,CAAA,YAAA,EAdA;AAeA,0BAAA,IAAA,CAAA,YAAA;AAfA,WAAA;AAiBA,UAAA,IAAA,CAAA,IAAA,CAAA,IAAA;AACA,SAnBA;AAoBA,QAAA,KAAA,CAAA,KAAA,GAAA,IAAA,CAAA,MAAA;AACA,QAAA,KAAA,CAAA,SAAA,GAAA,IAAA;AACA,QAAA,KAAA,CAAA,WAAA,GAAA,KAAA;AACA,OA1BA;AA2BA,KAhCA;AAiCA,IAAA,QAjCA,sBAiCA;AACA,WAAA,MAAA,CAAA,WAAA,GAAA,CAAA;AACA,WAAA,SAAA;AACA,KApCA;AAqCA;AACA,IAAA,QAtCA,oBAsCA,SAtCA,EAsCA,GAtCA,EAsCA;AAAA;;AACA,WAAA,KAAA,CAAA,SAAA,EAAA,aAAA,GAAA,IAAA;AACA,WAAA,SAAA,CAAA,YAAA;AACA,QAAA,KAAA,CAAA,KAAA,CAAA,SAAA,EAAA,IAAA,CAAA,GAAA;AACA,OAFA;AAGA,KA3CA;AA4CA,IAAA,SA5CA,uBA4CA;AACA,WAAA,KAAA,CAAA,UAAA,CAAA,WAAA;AACA;AA9CA;AA1BA,CAAA","sourcesContent":["<template>\n    <div class=\"table-classic-wrapper\">\n        <el-card shadow=\"always\">\n            <!-- 操作栏 -->\n            <div class=\"control-btns\">\n                <el-button type=\"primary\" @click=\"toDialog('addSysAccount', '')\">+ 新增管理帐号</el-button>\n            </div>\n            <!-- 查询栏 -->\n            <el-form\n                ref=\"searchForm\"\n                :inline=\"true\"\n                :model=\"search\"\n                label-width=\"90px\"\n                class=\"search-form\"\n            >\n                <template>\n                    <el-row>\n                        <el-col :span=\"6\">\n                            <el-form-item label=\"区域\">\n                                <el-select v-model=\"search.area\" placeholder=\"请选择\">\n                                    <el-option v-for=\"item in areaData\"\n                                               :key=\"item.value\"\n                                               :label=\"item.label\"\n                                               :value=\"item.value\">\n                                    </el-option>\n                                </el-select>\n                            </el-form-item>\n                        </el-col>\n                    </el-row>\n                </template>\n            </el-form>\n            <!-- 表格栏 -->\n            <el-table\n                ref=\"multipleTable\"\n                v-loading=\"listLoading\"\n                :data=\"tableData\"\n                tooltip-effect=\"dark\"\n                style=\"width: 100%\"\n                size=\"medium\"\n            >\n                <el-table-column prop=\"id\" label=\"编号\" align=\"center\" width=\"50\"/>\n                <el-table-column prop=\"email\" label=\"邮箱\" align=\"center\" width=\"300\"/>\n                <el-table-column prop=\"name\" label=\"真实姓名\" align=\"center\" width=\"200\"/>\n                <el-table-column prop=\"photo\" label=\"权限\" align=\"center\" width=\"250\">\n                    <template slot-scope=\"scope\">\n                        <a @click=\"toDialog('editRobot', scope.row)\" style=\"color: #1E88C7\">查看</a>\n                    </template>\n                </el-table-column>\n                <el-table-column prop=\"enable\" label=\"状态\" align=\"center\" width=\"200\">\n                    <template slot-scope=\"scope\">\n                        <el-switch v-model=\"scope.row.enable\"/>\n                    </template>\n                </el-table-column>\n                <el-table-column label=\"操作\" align=\"center\">\n                    <template slot-scope=\"scope\">\n                        <el-button type=\"text\" @click=\"toDialog('addSysAccount', scope.row)\">编辑</el-button>\n                    </template>\n                </el-table-column>\n            </el-table>\n            <!-- 分页栏 -->\n            <Pagination :total=\"total\" :page.sync=\"search.page.currentPage\" :limit.sync=\"search.page.pageSize\"\n                        @pagination=\"fetchData\"/>\n\n            <!-- 编辑资料 -->\n            <addSysAccount ref=\"addSysAccount\" @fetchData=\"fetchData\"/>\n        </el-card>\n    </div>\n</template>\n\n<script>\nimport Pagination from '../../../components/Pagination'\nimport addSysAccount from './dialog/add-sys-account'\nimport {getAreaList} from '@/utils/common'\n\n\nexport default {\n    components: { Pagination, addSysAccount},\n    data() {\n        return {\n            // 数据列表加载动画\n            listLoading: true,\n            // 查询列表参数对象\n            search: {\n                uid: undefined,\n                app: undefined,\n                robotId: undefined,\n                page: {\n                    currentPage: 1,\n                    pageSize: 10\n                }\n            },\n            // 数据总条数\n            total: 0,\n            // 防止多次连续提交表单\n            isSubmit: false,\n            areaData : getAreaList(),\n        }\n    },\n    created() {\n        this.fetchData()\n    },\n    methods: {\n        // 获取数据列表\n        fetchData() {\n            const $this = this\n            this.listLoading = true\n            this.$service.admin.getAdminList(this.search, function (result){\n                const list = result.getAdminsList();\n                const data = []\n                list.forEach((item, index)=>{\n                    const json = {\n                        \"id\" : item.getId(),\n                        \"enable\" : item.getEnable(),\n                        \"email\" : item.getEmail(),\n                        \"password\" : item.getPassword(),\n                        \"name\" : item.getName(),\n                        \"note\" : item.getNote(),\n                        \"area_ids\" : item.getAreaIdsList(),\n                        \"app_ids\" : item.getAppIdsList(),\n                        \"modules\" : item.getModulesList(),\n                        \"login_at\" : item.getLoginAt(),\n                        \"login_ip\" : item.getLoginIp(),\n                        \"fail_count\" : item.getFailCount(),\n                        \"access_token\" : item.getAccessToken(),\n                        \"updated_at\" : item.getUpdatedAt(),\n                        \"created_at\" : item.getCreatedAt()\n                    }\n                    data.push(json)\n                })\n                $this.total = list.length\n                $this.tableData = data\n                $this.listLoading = false\n            })\n        },\n        onSearch() {\n            this.search.currentPage = 1\n            this.fetchData()\n        },\n        // 弹框\n        toDialog(component, row){\n            this.$refs[component].dialogVisible = true\n            this.$nextTick(()=>{\n                this.$refs[component].init(row)\n            })\n        },\n        resetForm() {\n            this.$refs.searchForm.resetFields()\n        }\n    }\n}\n</script>\n\n<style lang=\"less\">\n.table-classic-wrapper {\n    .el-card {\n        min-height: 656px;\n    }\n\n    .control-btns {\n        margin-bottom: 20px;\n    }\n\n    .search-form {\n        padding-top: 18px;\n        margin-bottom: 15px;\n        background-color: #f7f8fb;\n    }\n\n    .el-table thead {\n        font-weight: 600;\n\n        th {\n            background-color: #f2f3f7;\n        }\n    }\n\n    .dialog-form {\n        .el-input {\n            width: 380px;\n        }\n        .footer-item {\n            margin-top: 50px;\n            text-align: right;\n        }\n    }\n    .el-divider--horizontal{\n        width: 95%;\n        margin-left: 20px;\n        margin-right: 20px;\n    }\n    .search-box {\n        margin-left: 0;\n        text-align: center;\n        margin-top: 10px;\n        margin-bottom: 20px\n    }\n}\n</style>\n"],"sourceRoot":"src/views/manage/sys"}]}