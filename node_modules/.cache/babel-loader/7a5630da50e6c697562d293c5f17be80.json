       } else {\n            console.log(err)\n        }\n    })\n    return data;\n}\n\n\nexport function getAnchorLevel(){\n    return [{\n        value: '1',\n        label: '差劲'\n    }, {\n        value: '2',\n        label: '中等'\n    }, {\n        value: '3',\n        label: '良好'\n    }, {\n        value: '4',\n        label: '优秀'\n    }]\n}\n\n\nexport function loginOut() {\n    removeToken()\n    location.reload()\n}\n\n"]}]}