ApBA;AAqBA,MAAA,QAAA,EAAA,WAAA,CAAA,IAAA,CArBA;AAsBA,MAAA,OAAA,EAAA,UAAA,CAAA,IAAA,EAAA,IAAA,CAtBA;AAuBA,MAAA,SAAA,EAAA,YAAA,EAvBA;AAwBA,MAAA,WAAA,EAAA,UAAA,CAAA,IAAA,CAxBA;AAyBA,MAAA,aAAA,EAAA,gBAAA;AAzBA,KAAA;AA2BA,GA9BA;AA+BA,EAAA,OA/BA,qBA+BA;AACA,SAAA,UAAA,CAAA,KAAA,MAAA,CAAA,MAAA;AACA,SAAA,SAAA;AACA,GAlCA;AAmCA,EAAA,OAAA,EAAA;AACA;AACA,IAAA,SAFA,uBAEA;AACA,UAAA,KAAA,GAAA,IAAA;AACA,WAAA,WAAA,GAAA,IAAA;AACA,WAAA,QAAA,CAAA,KAAA,CAAA,UAAA,CAAA,KAAA,WAAA,EAAA,EAAA,UAAA,MAAA,EAAA;AACA,YAAA,IAAA,GAAA,MAAA,CAAA,cAAA,EAAA;AACA,YAAA,IAAA,GAAA,EAAA;AACA,QAAA,IAAA,CAAA,OAAA,CAAA,UAAA,IAAA,EAAA,KAAA,EAAA;AACA,cAAA,KAAA,GAAA,QAAA,CAAA,IAAA,CAAA,WAAA,EAAA,CAAA;;AACA,cAAA,CAAA,OAAA,CAAA,IAAA,CAAA,WAAA,EAAA,CAAA,EAAA;AACA,YAAA,KAAA,GAAA,IAAA,CAAA,WAAA,KAAA,GAAA,GAAA,KAAA;AACA;;AACA,cAAA,IAAA,GAAA;AACA,kBAAA,IAAA,CAAA,KAAA,EADA;AAEA,qBAAA,IAAA,CAAA,QAAA,EAFA;AAGA,mBAAA,UAAA,CAAA,KAAA,CAAA,OAAA,EAAA,IAAA,CAAA,QAAA,EAAA,CAHA;AAIA,sBAAA,IAAA,CAAA,SAAA,EAJA;AAKA,sBAAA,UAAA,CAAA,KAAA,CAAA,QAAA,EAAA,IAAA,CAAA,SAAA,EAAA,CALA;AAMA,0BAAA,IAAA,CAAA,aAAA,EANA;AAOA,uBAAA,UAAA,CAAA,KAAA,EAAA,IAAA,CAAA,UAAA,EAAA,CAPA;AAQA,0BAAA,KAAA,CAAA,gBAAA,CAAA,IAAA,CAAA,SAAA,EAAA,EAAA,IAAA,CAAA,aAAA,EAAA,CARA;AASA,uBAAA,IAAA,CAAA,UAAA,EATA;AAUA,2BAAA,KAAA,CAAA,eAAA,CAAA,IAAA,CAAA,cAAA,EAAA,CAVA;AAWA,oBAAA,IAAA,CAAA,OAAA,EAXA;AAYA,0BAAA,IAAA,CAAA,aAAA,EAZA;AAaA,sBAAA,IAAA,CAAA,SAAA,EAbA;AAcA,wBAAA,KAdA;AAeA,wBAAA,QAAA,CAAA,IAAA,CAAA,WAAA,EAAA,CAfA;AAgBA,sBAAA,IAAA,CAAA,SAAA,EAhBA;AAiBA,yBAAA,YAAA,CAAA,IAAA,CAAA,YAAA,EAAA,CAjBA;AAkBA,sBAAA,MAAA,CAAA,IAAA,CAAA,SAAA,EAAA,CAlBA;AAmBA,uBAAA,MAAA,CAAA,IAAA,CAAA,UAAA,EAAA,CAnBA;AAoBA,yBAAA,MAAA,CAAA,IAAA,CAAA,YAAA,EAAA,CApBA;AAqBA,8BAAA,IAAA,CAAA,iBAAA;AArBA,WAAA;AAuBA,UAAA,IAAA,CAAA,IAAA,CAAA,IAAA;AACA,SA7BA;AA8BA,QAAA,KAAA,CAAA,KAAA,GAAA,MAAA,CAAA,aAAA,EAAA;AACA,QAAA,KAAA,CAAA,SAAA,GAAA,IAAA;AACA,QAAA,KAAA,CAAA,WAAA,GAAA,KAAA;AACA,OApCA;AAqCA,KA1CA;AA2CA,IAAA,WA3CA,yBA2CA;AACA,UAAA,KAAA,GAAA,KAAA,MAAA;;AACA,UAAA,KAAA,CAAA,IAAA,CAAA,MAAA,GAAA,CAAA,EAAA;AACA,QAAA,KAAA,CAAA,gBAAA,GAAA,SAAA,CAAA,KAAA,MAAA,CAAA,IAAA,CAAA,CAAA,CAAA,CAAA;AACA,QAAA,KAAA,CAAA,cAAA,GAAA,OAAA,CAAA,KAAA,MAAA,CAAA,IAAA,CAAA,CAAA,CAAA,CAAA;AACA;;AACA,aAAA,KAAA;AACA,KAlDA;AAmDA,IAAA,QAnDA,sBAmDA;AACA,WAAA,MAAA,CAAA,IAAA,CAAA,WAAA,GAAA,CAAA;AACA,WAAA,SAAA;AACA,KAtDA;AAuDA,IAAA,SAvDA,uBAuDA;AACA,WAAA,KAAA,CAAA,UAAA,CAAA,WAAA;AACA,KAzDA;AA0DA,IAAA,cA1DA,0BA0DA,GA1DA,EA0DA;AACA,WAAA,MAAA,CAAA,IAAA,CAAA,QAAA,GAAA,GAAA,CAAA,KAAA;AACA,KA5DA;AA6DA,IAAA,eA7DA,2BA6DA,WA7DA,EA6DA;AACA,UAAA,IAAA,GAAA,EAAA;AACA,WAAA,aAAA,CAAA,OAAA,CAAA,UAAA,IAAA,EAAA;AACA,YAAA,IAAA,CAAA,KAAA,KAAA,WAAA,EAAA;AACA,UAAA,IAAA,GAAA,OAAA,CAAA,IAAA,CAAA,IAAA,CAAA,GAAA,IAAA,CAAA,KAAA,GAAA,IAAA,CAAA,IAAA;AACA;AACA,OAJA;AAKA,aAAA,IAAA;AACA,KArEA;AAsEA,IAAA,gBAtEA,4BAsEA,MAtEA,EAsEA,SAtEA,EAsEA;AACA,UAAA,IAAA,GAAA,EAAA;AACA,MAAA,iBAAA,CAAA,MAAA,CAAA,CAAA,OAAA,CAAA,UAAA,IAAA,EAAA;AACA,YAAA,IAAA,CAAA,KAAA,KAAA,SAAA,EAAA;AACA,UAAA,IAAA,GAAA,IAAA,CAAA,KAAA;AACA;AACA,OAJA;AAKA,aAAA,IAAA;AACA,KA9EA;AA+EA,IAAA,UA/EA,sBA+EA,GA/EA,EA+EA;AACA,WAAA,OAAA,GAAA,kBAAA,CAAA,GAAA,EAAA,IAAA,EAAA,IAAA,CAAA;AACA;AAjFA;AAnCA,CAAA","sourcesContent":["<template>\r\n    <div class=\"table-classic-wrapper\">\r\n        <el-card shadow=\"always\">\r\n            <!-- 查询栏 -->\r\n            <el-form\r\n                ref=\"searchForm\"\r\n                :inline=\"true\"\r\n                :model=\"search\"\r\n                label-width=\"90px\"\r\n                class=\"search-form\"\r\n            >\r\n                <el-form-item label=\"用户ID\" prop=\"userId\">\r\n                    <el-input v-model=\"search.userId\" type=\"number\" placeholder=\"用户ID\"/>\r\n                </el-form-item>\r\n                <el-form-item label=\"订单号\" prop=\"recordId\">\r\n                    <el-input v-model=\"search.recordId\" type=\"number\" placeholder=\"订单号\"/>\r\n                </el-form-item>\r\n                <el-form-item label=\"订单状态\" prop=\"payStatus\">\r\n                    <el-select v-model=\"search.payStatus\" placeholder=\"请选择\">\r\n                        <el-option v-for=\"item in payStatus\"\r\n                                   :key=\"item.value\"\r\n                                   :label=\"item.label\"\r\n                                   :value=\"item.value\">\r\n                        </el-option>\r\n                    </el-select>\r\n                </el-form-item>\r\n                <el-form-item label=\"只看订阅\">\r\n                    <el-switch v-model=\"search.isSubscription\"/>\r\n                </el-form-item>\r\n                <el-form-item style=\"padding-left: 20px\">\r\n                    <el-button @click=\"onSearch\" type=\"primary\" size=\"small\" style=\"width: 120px;\">查&nbsp;&nbsp;询</el-button>\r\n                </el-form-item>\r\n                <el-collapse accordion @change=\"isCollapse = !isCollapse\">\r\n                    <el-collapse-item>\r\n                        <template slot=\"title\">\r\n                            {{isCollapse ? '展开' : '收起'}}\r\n                        </template>\r\n                        <div>\r\n                            <el-form-item label=\"渠道订单号\" prop=\"payChannel\">\r\n                                <el-input v-model=\"search.payChannel\" placeholder=\"渠道订单号\"/>\r\n                            </el-form-item>\r\n                            <el-form-item label=\"支付方式\" prop=\"payType\">\r\n                                <el-select v-model=\"search.payType\" placeholder=\"请选择\">\r\n                                    <el-option v-for=\"item in payTypeList\"\r\n                                               :key=\"item.value\"\r\n                                               :label=\"item.label\"\r\n                                               :value=\"item.value\">\r\n                                    </el-option>\r\n                                </el-select>\r\n                            </el-form-item>\r\n                            <el-form-item label=\"区域\" prop=\"areaId\">\r\n                                <el-select v-model=\"search.areaId\" @change=\"changeArea\" :disabled=\"authAreaId !== 0\" placeholder=\"请选择\">\r\n                                    <el-option v-for=\"item in areaData\"\r\n                                               :key=\"item.value\"\r\n                                               :label=\"item.label\"\r\n                                               :value=\"item.value\">\r\n                                    </el-option>\r\n                                </el-select>\r\n                            </el-form-item>\r\n                            <el-form-item label=\"APP\" prop=\"appId\">\r\n                                <el-select v-model=\"search.appId\" placeholder=\"请选择\">\r\n                                    <el-option v-for=\"item in appList\"\r\n                                               :key=\"item.value\"\r\n                                               :label=\"item.label\"\r\n                                               :value=\"item.value\">\r\n                                        <span style=\"float: left\">{{ item.label }}</span>\r\n                                        <span v-if=\"item.os === 1\">\r\n                                            <i class=\"icon-android-fill\" style=\"float: right\"></i>\r\n                                        </span>\r\n                                        <span v-else-if=\"item.os === 2\">\r\n                                            <i class=\"icon-pingguo\" style=\"float: right\"></i>\r\n                                        </span>\r\n                                    </el-option>\r\n                                </el-select>\r\n                            </el-form-item>\r\n                            <el-form-item label=\"交易时间\" prop=\"createdStart\">\r\n                                <el-date-picker\r\n                                    v-model=\"search.date\"\r\n                                    type=\"daterange\"\r\n                                    range-separator=\"至\"\r\n                                    start-placeholder=\"开始日期\"\r\n                                    end-placeholder=\"结束日期\">\r\n                                </el-date-picker>\r\n                            </el-form-item>\r\n\r\n                        </div>\r\n                    </el-collapse-item>\r\n                </el-collapse>\r\n            </el-form>\r\n            <!-- 表格栏 -->\r\n            <el-table\r\n                ref=\"multipleTable\"\r\n                v-loading=\"listLoading\"\r\n                :data=\"tableData\"\r\n                tooltip-effect=\"dark\"\r\n                style=\"width: 100%\"\r\n                size=\"medium\"\r\n            >\r\n                <el-table-column prop=\"id\" label=\"订单ID\" align=\"center\" width=\"150\" />\r\n                <el-table-column prop=\"payOrderId\" label=\"渠道订单号\" align=\"center\" width=\"200\" />\r\n                <el-table-column prop=\"commodityId\" label=\"商品名称\" align=\"center\" width=\"200\"/>\r\n                <el-table-column prop=\"appId\" label=\"来源APP\" align=\"center\" width=\"140\">\r\n                    <template scope=\"scope\">\r\n                        <div slot=\"reference\">\r\n                            {{ scope.row.app.label }}\r\n                            <span v-if=\"scope.row.app.os === 1\">\r\n                                <i class=\"icon-android-fill\"></i>\r\n                            </span>\r\n                            <span v-else>\r\n                                <i class=\"icon-pingguo\"></i>\r\n                            </span>\r\n                        </div>\r\n                    </template>\r\n                </el-table-column>\r\n                <el-table-column prop=\"areaId\" label=\"区域\" align=\"center\" width=\"80\"/>\r\n                <el-table-column prop=\"userId\" label=\"用户ID\" align=\"center\" width=\"120\"/>\r\n\r\n                <el-table-column prop=\"payType\" label=\"支付方式\" align=\"center\" width=\"100\"/>\r\n                <el-table-column prop=\"payChannel\" label=\"支付渠道\" align=\"center\" width=\"100\"/>\r\n                <el-table-column prop=\"usdPrice\" label=\"美元价格\" align=\"center\" width=\"80\"/>\r\n                <el-table-column prop=\"paidAt\" label=\"支付时间\" align=\"center\" width=\"180\"/>\r\n                <el-table-column prop=\"payStatus\" label=\"支付状态\" align=\"center\" width=\"80\">\r\n                    <template scope=\"scope\">\r\n                        <div slot=\"reference\">\r\n                            <el-tag size=\"medium\">{{ scope.row.payStatus }}</el-tag>\r\n                        </div>\r\n                    </template>\r\n                </el-table-column>\r\n                <el-table-column prop=\"amount\" label=\"钻石数量\" align=\"center\" width=\"80\"/>\r\n                <el-table-column prop=\"country\" label=\"国家\" align=\"center\" width=\"80\"/>\r\n                <el-table-column prop=\"payPrice\" label=\"本地价格\" align=\"center\" width=\"150\"/>\r\n                <el-table-column prop=\"queryAt\" label=\"查询支付状态时间\" align=\"center\" width=\"180\"/>\r\n                <el-table-column prop=\"createdAt\" label=\"创建时间\" align=\"center\" width=\"170\"/>\r\n                <el-table-column prop=\"isSubscription\" label=\"是否订阅\" align=\"center\" width=\"120\">\r\n                    <template slot-scope=\"scope\">\r\n                        <el-switch v-model=\"scope.row.isSubscription\" disabled/>\r\n                    </template>\r\n                </el-table-column>\r\n                <el-table-column prop=\"anchorId\" label=\"关联的主播id\" align=\"center\" width=\"120\"/>\r\n                <el-table-column prop=\"commission\" label=\"主播佣金\" align=\"center\" width=\"120\"/>\r\n<!--                <el-table-column label=\"操作\" align=\"center\" width=\"170\" fixed=\"right\">-->\r\n<!--                    <template slot-scope=\"scope\">-->\r\n<!--                        <el-button type=\"text\" @click=\"toMakeOrder('dataInfo', scope.row)\">补单</el-button>-->\r\n<!--                        <el-button type=\"text\" @click=\"toRetreat(scope.$index, scope.row)\">退单</el-button>-->\r\n<!--                    </template>-->\r\n<!--                </el-table-column>-->\r\n            </el-table>\r\n            <!-- 分页栏 -->\r\n            <Pagination :total=\"total\" :page.sync=\"search.page.currentPage\" :limit.sync=\"search.page.pageSize\"\r\n                        @pagination=\"fetchData\" @changePageSize=\"changePageSize($event)\"/>\r\n        </el-card>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport \"@/assets/icon/iconfont.css\"\r\nimport Pagination from '../../../components/Pagination'\r\nimport {\r\n    getAreaList,\r\n    getPayStatus,\r\n    getAppList,\r\n    getArrName,\r\n    getAppName,\r\n    getPayType,\r\n    getCommodityList, getPayChannelList, getAppListByAreaId, getCurrentUserAreaId\r\n} from \"@/utils/dist\";\r\nimport {endUnix, startUnix, toDollar, toTime} from \"@/utils/util\";\r\nimport {isEmpty} from \"@/api/api\";\r\n\r\nexport default {\r\n    components: { Pagination },\r\n    data() {\r\n        return {\r\n            // 数据列表加载动画\r\n            listLoading: true,\r\n            // 查询列表参数对象\r\n            search: {\r\n                userId: undefined,\r\n                recordId: undefined,\r\n                date: [],\r\n                appId: undefined,\r\n                payChannel: undefined,\r\n                areaId: getCurrentUserAreaId(),\r\n                payStatus: 0,\r\n                isSubscription: false,\r\n                page: {\r\n                    currentPage: 1,\r\n                    pageSize: 10\r\n                }\r\n            },\r\n            total: 0,\r\n            authAreaId: getCurrentUserAreaId(),\r\n            isCollapse: true,\r\n            areaData: getAreaList(true),\r\n            appList: getAppList(true, true),\r\n            payStatus: getPayStatus(),\r\n            payTypeList: getPayType(true),\r\n            commodityList: getCommodityList()\r\n        }\r\n    },\r\n    created() {\r\n        this.changeArea(this.search.areaId)\r\n        this.fetchData()\r\n    },\r\n    methods: {\r\n        // 获取数据列表\r\n        fetchData() {\r\n            const $this = this\r\n            this.listLoading = true\r\n            this.$service.order.getPayList(this.handleParam(), function (result){\r\n                const list = result.getRecordsList()\r\n                const data = []\r\n                list.forEach((item, index)=>{\r\n                    let price = toDollar(item.getPayPrice())\r\n                    if(!isEmpty(item.getCurrency())){\r\n                        price = item.getCurrency() + \"-\" + price\r\n                    }\r\n                    const json = {\r\n                        \"id\" : item.getId(),\r\n                        \"appId\" : item.getAppId(),\r\n                        \"app\" : getAppName($this.appList, item.getAppId()),\r\n                        \"userId\" : item.getUserId(),\r\n                        \"areaId\" : getArrName($this.areaData, item.getAreaId()),\r\n                        \"commission\" : item.getCommission(),\r\n                        \"payType\" : getPayType(false, item.getPayType()),\r\n                        \"payChannel\" : $this.handlePayChannel(item.getAreaId(), item.getPayChannel()),\r\n                        \"country\" : item.getCountry(),\r\n                        \"commodityId\" : $this.handleCommodity(item.getCommodityId()),\r\n                        \"desc\" : item.getDesc(),\r\n                        \"payOrderId\": item.getPayOrderId(),\r\n                        \"payUrl\" : item.getPayUrl(),\r\n                        \"payPrice\" : price,\r\n                        \"usdPrice\" : toDollar(item.getUsdPrice()),\r\n                        \"amount\": item.getAmount(),\r\n                        \"payStatus\" : getPayStatus(item.getPayStatus()),\r\n                        \"paidAt\" : toTime(item.getPaidAt()),\r\n                        \"queryAt\": toTime(item.getQueryAt()),\r\n                        \"createdAt\" : toTime(item.getCreatedAt()),\r\n                        \"isSubscription\" : item.getIsSubscription()\r\n                    }\r\n                    data.push(json)\r\n                })\r\n                $this.total = result.getTotalCount()\r\n                $this.tableData = data\r\n                $this.listLoading = false\r\n            });\r\n        },\r\n        handleParam(){\r\n            let param = this.search;\r\n            if (param.date.length > 0){\r\n                param.createdStartUint = startUnix(this.search.date[0])\r\n                param.createdEndUint = endUnix(this.search.date[1])\r\n            }\r\n            return param\r\n        },\r\n        onSearch() {\r\n            this.search.page.currentPage = 1\r\n            this.fetchData()\r\n        },\r\n        resetForm() {\r\n            this.$refs.searchForm.resetFields()\r\n        },\r\n        changePageSize(msg){\r\n            this.search.page.pageSize = msg.limit\r\n        },\r\n        handleCommodity(commodityId){\r\n            let name = \"\";\r\n            this.commodityList.forEach(item => {\r\n                if(item.value === commodityId){\r\n                    name = isEmpty(item.name) ? item.label : item.name\r\n                }\r\n            })\r\n            return name\r\n        },\r\n        handlePayChannel(areaId, channelId){\r\n            let name = \"\"\r\n            getPayChannelList(areaId).forEach(item => {\r\n                if(item.value === channelId){\r\n                    name = item.label\r\n                }\r\n            })\r\n            return name\r\n        },\r\n        changeArea(val){\r\n            this.appList = getAppListByAreaId(val, true, true)\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\">\r\n.el-collapse-item__header {\r\n    border-top: 0px solid #eaeefb;\r\n    height: 34px;\r\n    box-sizing: border-box;\r\n    border-bottom-left-radius: 2px;\r\n    border-bottom-right-radius: 2px;\r\n    margin-top: -75px;\r\n    color: #277cda;\r\n    z-index: 999;\r\n    float: right;\r\n}\r\n.el-collapse-item__wrap{\r\n    background-color: #f7f8fb;\r\n}\r\n.el-collapse{\r\n    border-top: 0px solid #EBEEF5;\r\n    border-bottom: 0px solid #EBEEF5;\r\n}\r\n.el-collapse-item__content {\r\n    padding-bottom: 5px;\r\n}\r\n.table-classic-wrapper {\r\n    .el-card {\r\n        min-height: 656px;\r\n    }\r\n\r\n    .control-btns {\r\n        margin-bottom: 20px;\r\n    }\r\n\r\n    .search-form {\r\n        padding-top: 18px;\r\n        margin-bottom: 15px;\r\n        background-color: #f7f8fb;\r\n    }\r\n\r\n    .el-table thead {\r\n        font-weight: 600;\r\n\r\n        th {\r\n            background-color: #f2f3f7;\r\n        }\r\n    }\r\n\r\n    .dialog-form {\r\n        .el-input {\r\n            width: 380px;\r\n        }\r\n        .footer-item {\r\n            margin-top: 50px;\r\n            text-align: right;\r\n        }\r\n    }\r\n    .el-divider--horizontal{\r\n        width: 95%;\r\n        margin-left: 20px;\r\n        margin-right: 20px;\r\n    }\r\n    .search-box {\r\n        margin-left: 0;\r\n        text-align: center;\r\n        margin-top: 10px;\r\n        margin-bottom: 20px\r\n    }\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/manage/order"}]}