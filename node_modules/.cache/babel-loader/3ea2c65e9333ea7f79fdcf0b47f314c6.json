AAA;AACA,YAAA,IAAA,GAAA,MAAA,CAAA,eAAA,EAAA;AACA,YAAA,IAAA,GAAA,EAAA;AACA,QAAA,IAAA,CAAA,OAAA,CAAA,UAAA,IAAA,EAAA,KAAA,EAAA;AACA,cAAA,IAAA,GAAA;AACA,kBAAA,IAAA,CAAA,KAAA,EADA;AAEA,qBAAA,IAAA,CAAA,QAAA,EAFA;AAGA,mBAAA,UAAA,CAAA,KAAA,CAAA,OAAA,EAAA,IAAA,CAAA,QAAA,EAAA,CAHA;AAIA,sBAAA,IAAA,CAAA,SAAA,EAJA;AAKA,yBAAA,IAAA,CAAA,YAAA,EALA;AAMA,4BAAA,IAAA,CAAA,YAAA,OAAA,CAAA,GAAA,KAAA,GAAA,KANA;AAOA,wBAAA,IAAA,CAAA,WAAA,EAPA;AAQA,oBAAA,cAAA,CAAA,IAAA,CAAA,OAAA,EAAA,CARA;AASA,oBAAA,IAAA,CAAA,OAAA,EATA;AAUA,mBAAA,IAAA,CAAA,MAAA,EAVA;AAWA,sBAAA,IAAA,IAAA,CAAA,IAAA,CAAA,SAAA,KAAA,IAAA,EAAA,MAAA,CAAA,qBAAA;AAXA,WAAA;AAaA,UAAA,IAAA,CAAA,IAAA,CAAA,IAAA;AACA,SAfA;AAgBA,QAAA,KAAA,CAAA,KAAA,GAAA,MAAA,CAAA,aAAA,EAAA;AACA,QAAA,KAAA,CAAA,SAAA,GAAA,IAAA;AACA,QAAA,KAAA,CAAA,WAAA,GAAA,KAAA;AACA,OAtBA;AAuBA,KA/BA;AAgCA,IAAA,WAhCA,yBAgCA;AACA,UAAA,KAAA,GAAA,KAAA,MAAA;;AACA,UAAA,OAAA,KAAA,MAAA,CAAA,YAAA,IAAA,WAAA,EAAA;AACA,QAAA,KAAA,CAAA,gBAAA,GAAA,KAAA,MAAA,CAAA,YAAA,CAAA,OAAA,KAAA,IAAA;AACA;;AACA,UAAA,OAAA,KAAA,MAAA,CAAA,UAAA,IAAA,WAAA,EAAA;AACA,QAAA,KAAA,CAAA,cAAA,GAAA,KAAA,MAAA,CAAA,UAAA,CAAA,OAAA,KAAA,IAAA;AACA;;AACA,aAAA,KAAA;AACA,KAzCA;AA0CA,IAAA,SA1CA,uBA0CA;AACA,WAAA,KAAA,CAAA,QAAA,CAAA,WAAA;AACA,KA5CA;AA6CA,IAAA,WA7CA,yBA6CA;AACA,WAAA,aAAA,GAAA,KAAA;AACA,WAAA,SAAA;AACA,WAAA,KAAA,CAAA,WAAA;AACA;AAjDA;AAzBA,CAAA","sourcesContent":["<template>\r\n    <el-dialog title=\"账户状态\" :visible.sync=\"dialogVisible\" append-to-body width=\"80%\" :before-close=\"closeDialog\">\r\n        <div class=\"table-classic-wrapper\">\r\n            <el-card shadow=\"always\">\r\n                <!-- 表格栏 -->\r\n                <el-table\r\n                    ref=\"multipleTable\"\r\n                    v-loading=\"listLoading\"\r\n                    :data=\"tableData\"\r\n                    tooltip-effect=\"dark\"\r\n                    style=\"width: 100%\"\r\n                    size=\"medium\"\r\n                >\r\n                    <el-table-column prop=\"app\" label=\"来源App\" align=\"center\" width=\"120\">\r\n                        <template scope=\"scope\">\r\n                            <div slot=\"reference\">\r\n                                {{ scope.row.app.label }}\r\n                                <span v-if=\"scope.row.app.os === 1\">\r\n                                <i class=\"icon-android-fill\"></i>\r\n                            </span>\r\n                                <span v-else>\r\n                                <i class=\"icon-pingguo\"></i>\r\n                            </span>\r\n                            </div>\r\n                        </template>\r\n                    </el-table-column>\r\n                    <el-table-column prop=\"userId\" label=\"用户id\" align=\"center\" width=\"120\" />\r\n                    <el-table-column prop=\"directionStr\" label=\"消息方向\" align=\"center\" width=\"120\"/>\r\n                    <el-table-column prop=\"anchorId\" label=\"主播id\" align=\"center\" width=\"120\" />\r\n                    <el-table-column prop=\"type\" label=\"消息类型\" align=\"center\" width=\"120\">\r\n                        <template scope=\"scope\">\r\n                            <div slot=\"reference\">\r\n                                <el-tag size=\"medium\">{{ scope.row.type }}</el-tag>\r\n                            </div>\r\n                        </template>\r\n                    </el-table-column>\r\n                    <el-table-column prop=\"text\" label=\"私信内容\" align=\"center\"  />\r\n                    <el-table-column prop=\"sendAt\" label=\"发送时间\" align=\"center\" width=\"220\"/>\r\n                </el-table>\r\n                <!-- 分页栏 -->\r\n                <Pagination :total=\"total\" :page.sync=\"search.page.currentPage\" :limit.sync=\"search.page.pageSize\"\r\n                            @pagination=\"fetchData\"/>\r\n            </el-card>\r\n        </div>\r\n    </el-dialog>\r\n</template>\r\n\r\n<script>\r\nimport { getTableList } from '../../../../api/api'\r\nimport Pagination from '../../../../components/Pagination'\r\nimport {getAppName, getMessageType} from \"@/utils/common\";\r\n\r\nexport default {\r\n    name: 'Table',\r\n    components: { Pagination },\r\n    data() {\r\n        return {\r\n            listLoading: true,\r\n            search: {\r\n                areaId: undefined,\r\n                userId: undefined,\r\n                anchorId: undefined,\r\n                appId: undefined,\r\n                direction: 0,\r\n                type: undefined,\r\n                keyword: undefined,\r\n                createdStart: undefined,\r\n                createdEnd: undefined,\r\n                page:{\r\n                    currentPage: 1,\r\n                    pageSize: 10\r\n                }\r\n            },\r\n            total: 0,\r\n            dialogVisible: false\r\n        }\r\n    },\r\n    methods: {\r\n        init(uid){\r\n            this.fetchData()\r\n        },\r\n        // 获取数据列表\r\n        fetchData() {\r\n            const $this = this\r\n            this.listLoading = true\r\n            this.$service.chat.getChatMessageList(this.handleParam(), function (result){\r\n                const list = result.getMessagesList();\r\n                const data = []\r\n                list.forEach((item, index)=>{\r\n                    const json = {\r\n                        \"id\" : item.getId(),\r\n                        \"appId\" : item.getAppId(),\r\n                        \"app\" : getAppName($this.appList, item.getAppId()),\r\n                        \"userId\" : item.getUserId(),\r\n                        \"direction\" : item.getDirection(),\r\n                        \"directionStr\" : item.getDirection() === 1 ? \"<--\" : \"-->\",\r\n                        \"anchorId\" : item.getAnchorId(),\r\n                        \"type\" : getMessageType(item.getType()),\r\n                        \"text\" : item.getText(),\r\n                        \"uri\" : item.getUri(),\r\n                        \"sendAt\" : new Date(item.getSendAt()*1000).format('yyyy-MM-dd hh:mm:ss')\r\n                    }\r\n                    data.push(json)\r\n                })\r\n                $this.total = result.getTotalCount()\r\n                $this.tableData = data\r\n                $this.listLoading = false\r\n            })\r\n        },\r\n        handleParam(){\r\n            let param = this.search;\r\n            if (typeof(this.search.createdStart) != \"undefined\"){\r\n                param.createdStartUint = this.search.createdStart.getTime() / 1000\r\n            }\r\n            if (typeof(this.search.createdEnd) != \"undefined\"){\r\n                param.createdEndUint = this.search.createdEnd.getTime() / 1000\r\n            }\r\n            return param\r\n        },\r\n        resetForm() {\r\n            this.$refs.ruleForm.resetFields()\r\n        },\r\n        closeDialog() {\r\n            this.dialogVisible = false\r\n            this.resetForm()\r\n            this.$emit('fetchData');\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\">\r\n\r\n</style>\r\n"],"sourceRoot":"src/views/manage/aia/dialog"}]}