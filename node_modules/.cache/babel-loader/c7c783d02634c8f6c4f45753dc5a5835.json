,KAAA,KAAA;AACA,OAHA;AAIA,MAAA,GAJA,eAIA,GAJA,EAIA;AACA,aAAA,IAAA,CAAA,cAAA,EAAA,GAAA;AACA;AANA;AATA,GA/BA;AAiDA,EAAA,OAAA,EAAA;AACA,IAAA,gBADA,4BACA,GADA,EACA;AACA,WAAA,KAAA,CAAA,gBAAA,EAAA;AAAA,QAAA,IAAA,EAAA,KAAA,WAAA;AAAA,QAAA,KAAA,EAAA;AAAA,OAAA;AACA,WAAA,KAAA,CAAA,YAAA,EAAA;AAAA,QAAA,IAAA,EAAA,KAAA,WAAA;AAAA,QAAA,KAAA,EAAA;AAAA,OAAA;AACA,KAJA;AAKA,IAAA,mBALA,+BAKA,GALA,EAKA;AACA,WAAA,KAAA,CAAA,YAAA,EAAA;AAAA,QAAA,IAAA,EAAA,GAAA;AAAA,QAAA,KAAA,EAAA,KAAA;AAAA,OAAA;AACA;AAPA;AAjDA,CAAA","sourcesContent":["<template>\r\n    <div class=\"pagination-wrapper\">\r\n        <el-pagination\r\n            :background=\"background\"\r\n            :current-page.sync=\"currentPage\"\r\n            :page-sizes=\"pageSizes\"\r\n            :page-size.sync=\"pageSize\"\r\n            :total=\"total\"\r\n            :layout=\"layout\"\r\n            @size-change=\"handleSizeChange\"\r\n            @current-change=\"handleCurrentChange\"\r\n        />\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n    name: 'Pagination',\r\n    props: {\r\n        page: {\r\n            type: Number,\r\n            default: 1,\r\n            required: true\r\n        },\r\n        limit: {\r\n            type: Number,\r\n            default: 10,\r\n            required: true\r\n        },\r\n        background: {\r\n            type: Boolean,\r\n            default: true\r\n        },\r\n        pageSizes: {\r\n            type: Array,\r\n            default: () => [5, 10, 15, 20, 50, 1000]\r\n        },\r\n        total: {\r\n            type: Number,\r\n            default: 0,\r\n            required: true\r\n        },\r\n        layout: {\r\n            type: String,\r\n            default: 'total, prev, pager, next, sizes, jumper'\r\n        }\r\n    },\r\n    computed: {\r\n        currentPage: {\r\n            get() {\r\n                return this.page\r\n            },\r\n            set(val) {\r\n                this.$emit('update:page', val)\r\n            }\r\n        },\r\n        pageSize: {\r\n            get() {\r\n                return this.limit\r\n            },\r\n            set(val) {\r\n                this.emit('update:limit', val)\r\n            }\r\n        }\r\n    },\r\n    methods: {\r\n        handleSizeChange(val) {\r\n            this.$emit('changePageSize', {page: this.currentPage, limit: val})\r\n            this.$emit('pagination', {page: this.currentPage, limit: val})\r\n        },\r\n        handleCurrentChange(val) {\r\n            this.$emit('pagination', {page: val, limit: this.pageSize})\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\">\r\n.pagination-wrapper {\r\n    margin-top: 25px;\r\n\r\n    .el-pagination {\r\n        float: right;\r\n    }\r\n}\r\n</style>\r\n"],"sourceRoot":"src/components/Pagination"}]}