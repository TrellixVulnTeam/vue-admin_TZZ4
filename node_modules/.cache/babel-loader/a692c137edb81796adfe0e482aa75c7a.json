,KAAA,GAAA,IAAA,CAAA,IAAA,CAAA,KAAA;AACA,UAAA,KAAA,CAAA,SAAA,GAAA,IAAA,CAAA,IAAA,CAAA,IAAA;AACA,UAAA,KAAA,CAAA,WAAA,GAAA,KAAA;AACA;AACA,OAPA,EAOA,KAPA,CAOA,YAAA;AACA,QAAA,KAAA,CAAA,WAAA,GAAA,KAAA;AACA,OATA;AAUA,KApBA;AAqBA,IAAA,MArBA,kBAqBA,GArBA,EAqBA;AACA,WAAA,aAAA,GAAA,IAAA;AACA,KAvBA;AAwBA,IAAA,OAxBA,mBAwBA,GAxBA,EAwBA;AACA,WAAA,cAAA,GAAA,IAAA;AACA,KA1BA;AA2BA;AACA,IAAA,MA5BA,kBA4BA,KA5BA,EA4BA,GA5BA,EA4BA;AAAA;;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA,EAAA,GAAA;AACA,WAAA,QAAA,CAAA,OAAA,EAAA,IAAA,EAAA;AACA,QAAA,iBAAA,EAAA,IADA;AAEA,QAAA,gBAAA,EAAA,IAFA;AAGA,QAAA,IAAA,EAAA;AAHA,OAAA,EAIA,IAJA,CAIA,YAAA;AACA,QAAA,MAAA,CAAA,QAAA,CAAA;AACA,UAAA,IAAA,EAAA,SADA;AAEA,UAAA,OAAA,EAAA;AAFA,SAAA;AAIA,OATA,EASA,KATA,CASA,YAAA;AACA,QAAA,MAAA,CAAA,QAAA,CAAA;AACA,UAAA,IAAA,EAAA,MADA;AAEA,UAAA,OAAA,EAAA;AAFA,SAAA;AAIA,OAdA;AAeA,KA7CA;AA8CA,IAAA,WA9CA,yBA8CA;AACA,WAAA,aAAA,GAAA,KAAA;AACA;AAhDA;AAtBA,C","sourcesContent":["<template>\r\n    <el-dialog title=\"资料管理\" :visible.sync=\"dialogVisible\" append-to-body width=\"80%\" :before-close=\"handleClose\">\r\n        <div class=\"table-classic-wrapper\">\r\n            <el-card shadow=\"always\">\r\n                <!-- 表格栏 -->\r\n                <el-table\r\n                    ref=\"multipleTable\"\r\n                    v-loading=\"listLoading\"\r\n                    :data=\"tableData\"\r\n                    tooltip-effect=\"dark\"\r\n                    style=\"width: 100%\"\r\n                    size=\"medium\"\r\n                >\r\n                    <el-table-column prop=\"no\" label=\"编号\" align=\"center\" width=\"100\"/>\r\n                    <el-table-column prop=\"nickname\" label=\"昵称\" align=\"center\" width=\"300\"/>\r\n                    <el-table-column prop=\"photo\" label=\"头像\" align=\"center\" width=\"300\">\r\n                        <template scope=\"scope\">\r\n                            <el-image style=\"width: 50px; height: 50px\" :src=\"scope.row.photo\" contain></el-image>\r\n                        </template>\r\n                    </el-table-column>\r\n                    <el-table-column prop=\"status\" label=\"在线状态\" align=\"center\" width=\"200\">\r\n                        <template slot-scope=\"scope\">\r\n                            <div slot=\"reference\">\r\n                                <el-tag size=\"medium\">{{ scope.row.status }}</el-tag>\r\n                            </div>\r\n                        </template>\r\n                    </el-table-column>\r\n\r\n                    <el-table-column label=\"操作\" align=\"center\">\r\n                        <template slot-scope=\"scope\">\r\n                            <el-button type=\"text\" @click=\"show(scope.$index, scope.row)\">查看资料</el-button>\r\n                            <el-button type=\"text\" @click=\"passed(scope.$index, scope.row)\">通过</el-button>\r\n                            <el-button type=\"text\" @click=\"refuse(scope.$index, scope.row)\">拒绝</el-button>\r\n                            <el-button type=\"text\" @click=\"history(scope.$index, scope.row)\">审核历史</el-button>\r\n                        </template>\r\n                    </el-table-column>\r\n                </el-table>\r\n                <!-- 分页栏 -->\r\n                <Pagination :total=\"total\" :page.sync=\"listQuery.currentPage\" :limit.sync=\"listQuery.pageSize\"\r\n                            @pagination=\"fetchData\"/>\r\n\r\n                <el-dialog\r\n                    title=\"拒绝\"\r\n                    :visible.sync=\"refuseVisible\"\r\n                    :append-to-body=\"true\">\r\n                    <div class=\"content-item\">\r\n                        <el-form ref=\"ruleForm\" :model=\"form\" label-width=\"150px\" class=\"form-list\">\r\n                            <el-form-item label=\"拒绝理由：\" prop=\"reason\">\r\n                                <el-input\r\n                                    v-model=\"form.reason\"\r\n                                    type=\"textarea\"\r\n                                    :autosize=\"{ minRows: 3, maxRows: 5 }\"\r\n                                    placeholder=\"请输入内容\"\r\n                                    maxlength=\"50\"\r\n                                    show-word-limit\r\n                                />\r\n                            </el-form-item>\r\n\r\n                            <el-form-item class=\"submit-box\">\r\n                                <el-button type=\"primary\" @click=\"submitForm('form')\" style=\"margin-right: 50px\">提&nbsp;&nbsp;&nbsp;交</el-button>\r\n                                <el-button @click=\"resetForm('form')\">重&nbsp;&nbsp;&nbsp;置</el-button>\r\n                            </el-form-item>\r\n                        </el-form>\r\n                    </div>\r\n                </el-dialog>\r\n\r\n                <el-dialog\r\n                    title=\"审核历史\"\r\n                    :visible.sync=\"historyVisible\"\r\n                    :append-to-body=\"true\">\r\n                    <!-- 表格栏 -->\r\n                    <el-table\r\n                        ref=\"multipleTable\"\r\n                        v-loading=\"listLoading\"\r\n                        :data=\"tableData\"\r\n                        tooltip-effect=\"dark\"\r\n                        style=\"width: 100%\"\r\n                        size=\"medium\"\r\n                    >\r\n                        <el-table-column prop=\"time\" label=\"日期\" align=\"center\"/>\r\n                        <el-table-column prop=\"status\" label=\"审核状态\" align=\"center\" width=\"200\"/>\r\n                        <el-table-column prop=\"reason\" label=\"拒绝原因\" align=\"center\"/>\r\n                    </el-table>\r\n                </el-dialog>\r\n            </el-card>\r\n        </div>\r\n    </el-dialog>\r\n</template>\r\n\r\n<script>\r\nimport { getTableList } from '../../../../api'\r\nimport Pagination from '../../../../components/Pagination'\r\nimport VueVideoPlayer from '../../../../components/VueVideoPlayer'\r\n\r\nexport default {\r\n    name: 'Table',\r\n    components: { Pagination, VueVideoPlayer },\r\n    data() {\r\n        return {\r\n            form: {\r\n\r\n            },\r\n            // 数据列表加载动画\r\n            listLoading: true,\r\n            // 查询列表参数对象\r\n            listQuery: {},\r\n            // 数据总条数\r\n            total: 0,\r\n            dialogVisible: false,\r\n            refuseVisible: false,\r\n            historyVisible: false\r\n        }\r\n    },\r\n    created() {\r\n        this.fetchData()\r\n    },\r\n    methods: {\r\n        init(uid){\r\n            console.log(\"init uid:\" + uid);\r\n            this.fetchData()\r\n        },\r\n        // 获取数据列表\r\n        fetchData() {\r\n            this.listLoading = true\r\n            let url = \"http://localhost:8000/data/data-list.json\"\r\n            // 获取数据列表接口\r\n            getTableList(this.listQuery, url).then(res => {\r\n                const data = res.data\r\n                if (data.code === 0) {\r\n                    this.total = data.data.total\r\n                    this.tableData = data.data.list\r\n                    this.listLoading = false\r\n                }\r\n            }).catch(() => {\r\n                this.listLoading = false\r\n            })\r\n        },\r\n        refuse(row) {\r\n            this.refuseVisible = true\r\n        },\r\n        history(row){\r\n            this.historyVisible = true\r\n        },\r\n        // 通过\r\n        passed(index, row) {\r\n            console.log(index, row)\r\n            this.$confirm('是否通过?', '提示', {\r\n                confirmButtonText: '确定',\r\n                cancelButtonText: '取消',\r\n                type: 'warning'\r\n            }).then(() => {\r\n                this.$message({\r\n                    type: 'success',\r\n                    message: '已通过!'\r\n                })\r\n            }).catch(() => {\r\n                this.$message({\r\n                    type: 'info',\r\n                    message: '已取消'\r\n                })\r\n            })\r\n        },\r\n        handleClose(){\r\n            this.dialogVisible = false\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\">\r\n.video-player-wrapper {\r\n    .content-box {\r\n        height: 440px;\r\n    }\r\n\r\n    .content-item {\r\n        padding: 8px 0;\r\n\r\n        .video-js {\r\n            .vjs-big-play-button {\r\n                top: 45%;\r\n                left: 45%;\r\n                font-size: 2em;\r\n            }\r\n        }\r\n    }\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/manage/anchor/dialog"}]}