,CAAA,QAAA,GAAA,KAAA,MAAA,CAAA,KAAA,CAAA,QAAA;;AACA,YAAA,OAAA,KAAA,MAAA,CAAA,KAAA,KAAA,WAAA,EAAA;AACA,eAAA,SAAA;AACA;AACA,OAPA;AAQA,MAAA,IAAA,EAAA;AARA;AADA,GAvBA;AAmCA,EAAA,OAAA,EAAA;AACA;AACA,IAAA,SAFA,uBAEA;AACA,UAAA,KAAA,GAAA,IAAA;AACA,WAAA,WAAA,GAAA,IAAA;AACA,WAAA,QAAA,CAAA,MAAA,CAAA,oBAAA,CAAA,KAAA,MAAA,EAAA,UAAA,MAAA,EAAA;AACA,YAAA,IAAA,GAAA,MAAA,CAAA,cAAA,EAAA;AACA,YAAA,IAAA,GAAA,EAAA;AACA,QAAA,IAAA,CAAA,OAAA,CAAA,UAAA,IAAA,EAAA,KAAA,EAAA;AACA,cAAA,IAAA,GAAA;AACA,kBAAA,IAAA,CAAA,KAAA,EADA;AAEA,sBAAA,IAAA,CAAA,SAAA,EAFA;AAGA,uBAAA,UAAA,CAAA,KAAA,CAAA,QAAA,EAAA,IAAA,CAAA,SAAA,EAAA,CAHA;AAIA,uBAAA,IAAA,CAAA,UAAA,EAJA;AAKA,sBAAA,IAAA,CAAA,SAAA,EALA;AAMA,qBAAA,IAAA,CAAA,QAAA,EANA;AAOA,oBAAA,IAAA,CAAA,OAAA,EAPA;AAQA,wBAAA,IAAA,CAAA,WAAA,EARA;AASA,oBAAA,IAAA,CAAA,OAAA,EATA;AAUA,4BAAA,IAAA,CAAA,eAAA,EAVA;AAWA,8BAAA,IAAA,CAAA,qBAAA,EAXA;AAYA,6BAAA,IAAA,CAAA,oBAAA,EAZA;AAaA,4BAAA,KAAA,CAAA,gBAAA,CAAA,IAAA,CAAA,SAAA,EAAA,EAAA,IAAA,CAAA,oBAAA,EAAA,EAAA,IAAA,CAAA,IAAA,CAbA;AAcA,oBAAA,IAAA,CAAA,OAAA,EAdA;AAeA,sBAAA;AAfA,WAAA;AAiBA,UAAA,IAAA,CAAA,IAAA,CAAA,IAAA;AACA,SAnBA;AAoBA,QAAA,KAAA,CAAA,KAAA,GAAA,IAAA,CAAA,MAAA;AACA,QAAA,KAAA,CAAA,SAAA,GAAA,IAAA;AACA,QAAA,KAAA,CAAA,WAAA,GAAA,KAAA;AACA,OA1BA;AA2BA,KAhCA;AAiCA,IAAA,QAjCA,sBAiCA;AACA,WAAA,MAAA,CAAA,IAAA,CAAA,WAAA,GAAA,CAAA;AACA,WAAA,SAAA;AACA,KApCA;AAqCA,IAAA,QArCA,oBAqCA,SArCA,EAqCA,GArCA,EAqCA;AAAA;;AACA,WAAA,KAAA,CAAA,SAAA,EAAA,aAAA,GAAA,IAAA;AACA,WAAA,SAAA,CAAA,YAAA;AACA,QAAA,KAAA,CAAA,KAAA,CAAA,SAAA,EAAA,IAAA,CAAA,KAAA,CAAA,MAAA,CAAA,OAAA;AACA,OAFA;AAGA,KA1CA;AA2CA,IAAA,qBA3CA,iCA2CA,GA3CA,EA2CA;AACA,WAAA,iBAAA,GAAA,GAAA;AACA;AA7CA;AAnCA,CAAA","sourcesContent":["<template>\n    <div class=\"table-classic-wrapper\">\n        <Hints>\n            <template slot=\"hintName\"> 配置 < {{ search.areaName }} > 国家</template>\n        </Hints>\n        <el-card shadow=\"always\">\n            <!-- 操作栏 -->\n            <div class=\"control-btns\">\n                <el-button type=\"primary\" @click=\"toDialog('addCountry', '')\">+ 新增国家</el-button>\n            </div>\n            <!-- 表格栏 -->\n            <el-table\n                ref=\"multipleTable\"\n                v-loading=\"listLoading\"\n                :data=\"tableData\"\n                tooltip-effect=\"dark\"\n                style=\"width: 100%\"\n                size=\"medium\"\n            >\n                <el-table-column prop=\"id\" label=\"用户ID\" align=\"center\" width=\"120\" />\n                <el-table-column prop=\"areaStr\" label=\"区域\" align=\"center\" width=\"120\" />\n                <el-table-column prop=\"country\" label=\"国家代码\" align=\"center\" width=\"120\" />\n                <el-table-column prop=\"enable\" label=\"是否启用\" align=\"center\" width=\"120\">\n                    <template slot-scope=\"scope\">\n                        <el-switch v-model=\"scope.row.enable\" disabled/>\n                    </template>\n                </el-table-column>\n                <el-table-column prop=\"title\" label=\"国家名称(后台)\" align=\"center\" width=\"120\"/>\n                <el-table-column prop=\"name\" label=\"国家名称(客户端)\" align=\"center\" width=\"150\"/>\n                <el-table-column prop=\"currency\" label=\"货币类型\" align=\"center\" width=\"120\" />\n                <el-table-column prop=\"icon\" label=\"图标\" align=\"center\" width=\"150\">\n                    <template scope=\"scope\">\n                        <el-image fit=\"contain\" style=\"width: 50px; height: 50px\" :src=\"scope.row.icon\" :preview-src-list=\"[scope.row.icon]\"/>\n                    </template>\n                </el-table-column>\n                <el-table-column prop=\"exchangeRate\" label=\"汇率\" align=\"center\" :show-overflow-tooltip=\"true\"  width=\"150\" />\n                <el-table-column prop=\"channelNames\" label=\"启用的支付渠道\" align=\"center\" width=\"180\" />\n                <el-table-column prop=\"sort\" label=\"排序\" align=\"center\" width=\"120\" />\n                <el-table-column label=\"操作\" align=\"center\" width=\"180\" fixed=\"right\">\n                    <template slot-scope=\"scope\">\n                        <el-button type=\"text\" @click=\"toDialog('payConfig',scope.row)\">支付配置</el-button>\n                        <el-button type=\"text\" @click=\"toDialog('approximationConfig',scope.row)\">近似值</el-button>\n                        <el-button type=\"text\" @click=\"toDialog('addCountry',scope.row)\">更新</el-button>\n                    </template>\n                </el-table-column>\n            </el-table>\n            <!-- 分页栏 -->\n            <Pagination :total=\"total\" :page.sync=\"search.page.currentPage\" :limit.sync=\"search.page.pageSize\"\n                        @pagination=\"fetchData\"/>\n\n            <!-- 更新 弹出栏 -->\n            <addCountry ref=\"addCountry\" @fetchData=\"fetchData\"/>\n\n        </el-card>\n    </div>\n</template>\n\n<script>\nimport Pagination from '../../../components/Pagination'\nimport Hints from '../../../components/Hints'\nimport addCountry from './dialog/addCountry'\nimport {getAppList, getAreaList, getArrName} from \"@/utils/common\";\n\nexport default {\n    name: 'Table',\n    components: {Pagination, Hints, addCountry},\n    data() {\n        return {\n            listLoading: true,\n            search: {\n                areaId: undefined,\n                areaName: \"\",\n                page: {\n                    currentPage: 1,\n                    pageSize: 10\n                }\n            },\n            total: 0,\n            tableData: [],\n            multipleSelection: [],\n            formVisible: false,\n            appList: getAppList(),\n            areaList: getAreaList()\n        }\n    },\n    created() {},\n    watch: {\n        $route: {\n            handler() {\n                this.search.areaId = this.$route.query.areaId\n                this.search.areaName = this.$route.query.areaName\n                if (typeof(this.search.appId) !== \"undefined\"){\n                    this.fetchData()\n                }\n            },\n            deep: true\n        }\n    },\n    methods: {\n        // 获取数据列表\n        fetchData() {\n            const $this = this\n            this.listLoading = true\n            this.$service.config.getCountryConfigList(this.search, function (result){\n                const list = result.getConfigsList()\n                const data = []\n                list.forEach((item, index) => {\n                    const json = {\n                        \"id\" : item.getId(),\n                        \"areaId\" : item.getAreaId(),\n                        \"areaStr\" : getArrName($this.areaList, item.getAreaId()),\n                        \"country\" : item.getCountry(),\n                        \"enable\" : item.getEnable(),\n                        \"title\" : item.getTitle(),\n                        \"name\" : item.getName(),\n                        \"currency\" : item.getCurrency(),\n                        \"icon\" : item.getIcon(),\n                        \"exchangeRate\" : item.getExchangeRate(),\n                        \"approximations\" : item.getApproximationsList(),\n                        \"payChannelIds\" : item.getPayChannelIdsList(),\n                        \"channelNames\" : $this.handlePayChannel(item.getAreaId(), item.getPayChannelIdsList()).join(\", \"),\n                        \"sort\" : item.getSort(),\n                        \"struct\" : item\n                    }\n                    data.push(json)\n                })\n                $this.total = list.length\n                $this.tableData = data\n                $this.listLoading = false\n            });\n        },\n        onSubmit() {\n            this.search.page.currentPage = 1\n            this.fetchData()\n        },\n        toDialog(component, row){\n            this.$refs[component].dialogVisible = true\n            this.$nextTick(()=>{\n                this.$refs[component].init(this.search.robotId)\n            })\n        },\n        handleSelectionChange(val) {\n            this.multipleSelection = val\n        }\n    }\n}\n</script>\n\n<style lang=\"less\">\n.table-classic-wrapper {\n    .el-card {\n        min-height: 656px;\n    }\n\n    .control-btns {\n        margin-bottom: 20px;\n    }\n\n    .search-form {\n        padding-top: 18px;\n        margin-bottom: 15px;\n        background-color: #f7f8fb;\n    }\n\n    .el-table thead {\n        font-weight: 600;\n\n        th {\n            background-color: #f2f3f7;\n        }\n    }\n\n    .dialog-form {\n        .el-input {\n            width: 380px;\n        }\n\n        .footer-item {\n            margin-top: 50px;\n            text-align: right;\n        }\n    }\n\n    .upload-box,\n    .export-data {\n        width: 300px;\n        margin: 0 auto 30px;\n    }\n\n    .upload-box {\n        width: 156px;\n\n        .files-upload {\n            color: #20a0ff;\n        }\n    }\n\n    .hints {\n        font-size: 12px;\n        color: #aaa;\n        text-align: center;\n    }\n}\n</style>\n"],"sourceRoot":"src/views/manage/config"}]}