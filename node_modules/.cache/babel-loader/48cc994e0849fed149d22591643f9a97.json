YAAA,IAAA,GAAA,EAAA;AACA,QAAA,IAAA,CAAA,OAAA,CAAA,UAAA,IAAA,EAAA,KAAA,EAAA;AACA,cAAA,IAAA,GAAA;AACA,kBAAA,IAAA,CAAA,KAAA,EADA;AAEA,sBAAA,IAAA,CAAA,SAAA,EAFA;AAGA,uBAAA,UAAA,CAAA,KAAA,CAAA,QAAA,EAAA,IAAA,CAAA,SAAA,EAAA,CAHA;AAIA,oBAAA,IAAA,CAAA,OAAA,EAJA;AAKA,uBAAA,UAAA,CAAA,IAAA,CAAA,OAAA,EAAA,CALA;AAMA,qBAAA,IAAA,CAAA,QAAA,EANA;AAOA,mBAAA,UAAA,CAAA,KAAA,CAAA,OAAA,EAAA,IAAA,CAAA,QAAA,EAAA,CAPA;AAQA,yBAAA,IAAA,CAAA,YAAA,EARA;AASA,2BAAA,IAAA,CAAA,cAAA,EATA;AAUA,2BAAA,IAAA,CAAA,cAAA,EAVA;AAWA,sBAAA,IAAA,CAAA,SAAA,EAXA;AAYA,sBAAA,IAAA,CAAA,SAAA,EAZA;AAaA,sBAAA,IAAA,CAAA,SAAA,EAbA;AAcA,yBAAA,IAAA,CAAA,YAAA,EAdA;AAeA,sBAAA;AAfA,WAAA;AAiBA,UAAA,IAAA,CAAA,IAAA,CAAA,IAAA;AACA,SAnBA;AAoBA,QAAA,KAAA,CAAA,KAAA,GAAA,IAAA,CAAA,MAAA;AACA,QAAA,KAAA,CAAA,SAAA,GAAA,IAAA;AACA,QAAA,KAAA,CAAA,WAAA,GAAA,KAAA;AACA,OA1BA;AA2BA,KA/BA;AAgCA,IAAA,QAhCA,sBAgCA;AACA,WAAA,MAAA,CAAA,IAAA,CAAA,WAAA,GAAA,CAAA;AACA,WAAA,SAAA;AACA,KAnCA;AAoCA,IAAA,cApCA,0BAoCA,GApCA,EAoCA;AACA,WAAA,MAAA,CAAA,IAAA,CAAA,QAAA,GAAA,GAAA,CAAA,KAAA;AACA,KAtCA;AAuCA,IAAA,QAvCA,oBAuCA,SAvCA,EAuCA,GAvCA,EAuCA;AAAA;;AACA,WAAA,KAAA,CAAA,SAAA,EAAA,aAAA,GAAA,IAAA;AACA,WAAA,SAAA,CAAA,YAAA;AACA,QAAA,KAAA,CAAA,KAAA,CAAA,SAAA,EAAA,IAAA,CAAA,GAAA;AACA,OAFA;AAGA,KA5CA;AA6CA,IAAA,SA7CA,uBA6CA;AACA,WAAA,KAAA,CAAA,UAAA,CAAA,WAAA;AACA;AA/CA;AAtBA,CAAA","sourcesContent":["<template>\r\n    <div class=\"table-classic-wrapper\">\r\n        <el-card shadow=\"always\">\r\n            <!-- 操作栏 -->\r\n            <div class=\"control-btns\">\r\n                <el-button type=\"primary\" @click=\"toDialog('addPay', '')\">+ 新增支付参数</el-button>\r\n            </div>\r\n            <!-- 查询栏 -->\r\n            <el-form\r\n                ref=\"searchForm\"\r\n                :inline=\"true\"\r\n                :model=\"search\"\r\n                label-width=\"90px\"\r\n                class=\"search-form\"\r\n            >\r\n                <el-form-item label=\"区域\" prop=\"areaId\">\r\n                    <el-select v-model=\"search.areaId\" placeholder=\"请选择\">\r\n                        <el-option v-for=\"item in areaList\"\r\n                                   :key=\"item.value\"\r\n                                   :label=\"item.label\"\r\n                                   :value=\"item.value\">\r\n                        </el-option>\r\n                    </el-select>\r\n                </el-form-item>\r\n                <el-form-item style=\"padding-left: 20px\">\r\n                    <el-button @click=\"onSearch\" type=\"primary\" size=\"small\" style=\"width: 120px;\">查&nbsp;&nbsp;询</el-button>\r\n                </el-form-item>\r\n            </el-form>\r\n            <!-- 表格栏 -->\r\n            <el-table\r\n                ref=\"multipleTable\"\r\n                v-loading=\"listLoading\"\r\n                :data=\"tableData\"\r\n                tooltip-effect=\"dark\"\r\n                style=\"width: 100%\"\r\n                size=\"medium\"\r\n            >\r\n                <el-table-column prop=\"areaStr\" label=\"区域\" align=\"center\" width=\"120\" />\r\n                <el-table-column prop=\"typeStr\" label=\"支付方式\" align=\"center\" width=\"120\"/>\r\n                <el-table-column prop=\"appId\" label=\"第三方应用Id\" align=\"center\" width=\"200\"/>\r\n                <el-table-column prop=\"appSecret\" label=\"应用密匙\" align=\"center\" :show-overflow-tooltip=\"true\" width=\"200\"/>\r\n                <el-table-column prop=\"redirectUrl\" label=\"重定向地址\" align=\"center\" :show-overflow-tooltip=\"true\" width=\"200\"/>\r\n                <el-table-column prop=\"callbackUrl\" label=\"回调地址\" align=\"center\" :show-overflow-tooltip=\"true\" width=\"200\" />\r\n                <el-table-column prop=\"extra1\" label=\"字段1\" align=\"center\" :show-overflow-tooltip=\"true\" width=\"200\"/>\r\n                <el-table-column prop=\"extra2\" label=\"字段2\" align=\"center\" :show-overflow-tooltip=\"true\" width=\"200\" />\r\n                <el-table-column prop=\"extra3\" label=\"字段3\" align=\"center\" :show-overflow-tooltip=\"true\" width=\"200\" />\r\n                <el-table-column prop=\"createdAt\" label=\"创建时间\" align=\"center\" width=\"180\"/>\r\n                <el-table-column label=\"操作\" align=\"center\" width=\"180\" fixed=\"right\">\r\n                    <template slot-scope=\"scope\">\r\n                        <el-button type=\"text\" @click=\"toDialog('addPay',scope.row)\">更新</el-button>\r\n                        <el-button type=\"text\" @click=\"toDialog('delPay',scope.row)\">删除</el-button>\r\n                    </template>\r\n                </el-table-column>\r\n            </el-table>\r\n            <!-- 分页栏 -->\r\n            <Pagination :total=\"total\" :page.sync=\"search.page.currentPage\" :limit.sync=\"search.page.pageSize\"\r\n                        @pagination=\"fetchData\" @changePageSize=\"changePageSize($event)\"/>\r\n\r\n            <!-- 更新 弹出栏 -->\r\n            <addPay ref=\"addPay\" @fetchData=\"fetchData\"/>\r\n\r\n        </el-card>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n\r\nimport Pagination from '../../../components/Pagination'\r\nimport imageShow from '../../../components/ImageShow/image-show'\r\nimport addPay from './dialog/addPay'\r\nimport {getAreaList, getArrName, getPayType, getAppList, getAppName} from \"@/utils/common\";\r\n\r\nexport default {\r\n    components: { Pagination, imageShow, addPay},\r\n    data() {\r\n        return {\r\n            listLoading: true,\r\n            search: {\r\n                areaId: undefined,\r\n                page: {\r\n                    currentPage: 1,\r\n                    pageSize: 10\r\n                }\r\n            },\r\n            total: 0,\r\n            isCollapse: true,\r\n            areaList: getAreaList(),\r\n            payTypeList: getPayType(),\r\n            appList: getAppList()\r\n        }\r\n    },\r\n    created() {\r\n        this.fetchData()\r\n    },\r\n    methods: {\r\n        fetchData() {\r\n            const $this = this\r\n            this.listLoading = true\r\n            this.$service.config.getPayConfigList(this.search, function (result){\r\n                const list = result.getConfigsList()\r\n                const data = []\r\n                list.forEach((item, index) => {\r\n                    const json = {\r\n                        \"id\" : item.getId(),\r\n                        \"areaId\" : item.getAreaId(),\r\n                        \"areaStr\" : getArrName($this.areaList, item.getAreaId()),\r\n                        \"type\" : item.getType(),\r\n                        \"typeStr\" : getPayType(item.getType()),\r\n                        \"appId\" : item.getAppId(),\r\n                        \"app\" : getAppName($this.appList, item.getAppId()),\r\n                        \"appSecret\" : item.getAppSecret(),\r\n                        \"redirectUrl\" : item.getRedirectUrl(),\r\n                        \"callbackUrl\" : item.getCallbackUrl(),\r\n                        \"extra1\" : item.getExtra1(),\r\n                        \"extra2\" : item.getExtra2(),\r\n                        \"extra3\" : item.getExtra3(),\r\n                        \"createdAt\" : item.getCreatedAt(),\r\n                        \"struct\" : item\r\n                    }\r\n                    data.push(json)\r\n                })\r\n                $this.total = list.length\r\n                $this.tableData = data\r\n                $this.listLoading = false\r\n            });\r\n        },\r\n        onSearch() {\r\n            this.search.page.currentPage = 1\r\n            this.fetchData()\r\n        },\r\n        changePageSize(msg){\r\n            this.search.page.pageSize = msg.limit\r\n        },\r\n        toDialog(component, row){\r\n            this.$refs[component].dialogVisible = true\r\n            this.$nextTick(()=>{\r\n                this.$refs[component].init(row);\r\n            })\r\n        },\r\n        resetForm() {\r\n            this.$refs.searchForm.resetFields()\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\">\r\n.table-classic-wrapper {\r\n    .el-card {\r\n        min-height: 656px;\r\n    }\r\n\r\n    .control-btns {\r\n        margin-bottom: 20px;\r\n    }\r\n\r\n    .search-form {\r\n        padding-top: 18px;\r\n        margin-bottom: 15px;\r\n        background-color: #f7f8fb;\r\n    }\r\n\r\n    .el-table thead {\r\n        font-weight: 600;\r\n\r\n        th {\r\n            background-color: #f2f3f7;\r\n        }\r\n    }\r\n\r\n    .dialog-form {\r\n        .el-input {\r\n            width: 380px;\r\n        }\r\n\r\n        .footer-item {\r\n            margin-top: 50px;\r\n            text-align: right;\r\n        }\r\n    }\r\n    .el-divider--horizontal{\r\n        width: 95%;\r\n        margin-left: 20px;\r\n        margin-right: 20px;\r\n    }\r\n    .search-box {\r\n        margin-left: 0;\r\n        text-align: center;\r\n        margin-top: 10px;\r\n        margin-bottom: 20px\r\n    }\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/manage/config"}]}