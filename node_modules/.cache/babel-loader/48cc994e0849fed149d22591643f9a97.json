A,MAAA,EAAA;AACA,YAAA,IAAA,GAAA,MAAA,CAAA,eAAA,EAAA;AACA,YAAA,IAAA,GAAA,EAAA;AACA,QAAA,IAAA,CAAA,OAAA,CAAA,UAAA,IAAA,EAAA,KAAA,EAAA;AACA,cAAA,IAAA,GAAA;AACA,kBAAA,IAAA,CAAA,KAAA,EADA;AAEA,sBAAA,IAAA,CAAA,SAAA,EAFA;AAGA,uBAAA,UAAA,CAAA,KAAA,CAAA,QAAA,EAAA,IAAA,CAAA,SAAA,EAAA,CAHA;AAIA,sBAAA,IAAA,CAAA,SAAA,EAJA;AAKA,yBAAA,SAAA,CAAA,IAAA,CAAA,SAAA,EAAA,CALA;AAMA,uBAAA,IAAA,CAAA,UAAA,EANA;AAOA,uBAAA,IAAA,CAAA,UAAA,EAPA;AAQA,0BAAA,UAAA,CAAA,IAAA,CAAA,UAAA,EAAA,CARA;AASA,qBAAA,IAAA,CAAA,QAAA,EATA;AAUA,oBAAA,IAAA,CAAA,OAAA,EAVA;AAWA,oBAAA,IAAA,CAAA,OAAA,EAXA;AAYA,sBAAA,IAAA,CAAA,SAAA,EAZA;AAaA,wBAAA,IAAA,CAAA,WAAA,EAbA;AAcA,oBAAA,IAAA,CAAA,OAAA,EAdA;AAeA,oBAAA,IAAA,CAAA,OAAA,EAfA;AAgBA,sBAAA;AAhBA,WAAA;AAkBA,UAAA,IAAA,CAAA,IAAA,CAAA,IAAA;AACA,SApBA;AAqBA,QAAA,KAAA,CAAA,KAAA,GAAA,IAAA,CAAA,MAAA;AACA,QAAA,KAAA,CAAA,SAAA,GAAA,IAAA;AACA,QAAA,KAAA,CAAA,WAAA,GAAA,KAAA;AACA,OA3BA;AA4BA,KAhCA;AAiCA,IAAA,QAjCA,sBAiCA;AACA,WAAA,MAAA,CAAA,IAAA,CAAA,WAAA,GAAA,CAAA;AACA,WAAA,SAAA;AACA,KApCA;AAqCA,IAAA,cArCA,0BAqCA,GArCA,EAqCA;AACA,WAAA,MAAA,CAAA,IAAA,CAAA,QAAA,GAAA,GAAA,CAAA,KAAA;AACA,KAvCA;AAwCA,IAAA,QAxCA,oBAwCA,SAxCA,EAwCA,GAxCA,EAwCA;AAAA;;AACA,WAAA,KAAA,CAAA,SAAA,EAAA,aAAA,GAAA,IAAA;AACA,WAAA,SAAA,CAAA,YAAA;AACA,QAAA,KAAA,CAAA,KAAA,CAAA,SAAA,EAAA,IAAA,CAAA,GAAA;AACA,OAFA;AAGA,KA7CA;AA8CA,IAAA,SA9CA,uBA8CA;AACA,WAAA,KAAA,CAAA,UAAA,CAAA,WAAA;AACA;AAhDA;AAtBA,CAAA","sourcesContent":["<template>\r\n    <div class=\"table-classic-wrapper\">\r\n        <el-card shadow=\"always\">\r\n            <!-- 操作栏 -->\r\n            <div class=\"control-btns\">\r\n                <el-button type=\"primary\" @click=\"toDialog('addPay', '')\">+ 新增支付</el-button>\r\n            </div>\r\n            <!-- 查询栏 -->\r\n            <el-form\r\n                ref=\"searchForm\"\r\n                :inline=\"true\"\r\n                :model=\"search\"\r\n                label-width=\"90px\"\r\n                class=\"search-form\"\r\n            >\r\n                <el-form-item label=\"区域\" prop=\"areaId\">\r\n                    <el-select v-model=\"search.areaId\" placeholder=\"请选择\">\r\n                        <el-option v-for=\"item in areaList\"\r\n                                   :key=\"item.value\"\r\n                                   :label=\"item.label\"\r\n                                   :value=\"item.value\">\r\n                        </el-option>\r\n                    </el-select>\r\n                </el-form-item>\r\n                <el-form-item label=\"支付方式\" prop=\"areaId\">\r\n                    <el-select v-model=\"search.payType\" placeholder=\"请选择\">\r\n                        <el-option v-for=\"item in payTypeList\"\r\n                                   :key=\"item.value\"\r\n                                   :label=\"item.label\"\r\n                                   :value=\"item.value\">\r\n                        </el-option>\r\n                    </el-select>\r\n                </el-form-item>\r\n                <el-form-item style=\"padding-left: 20px\">\r\n                    <el-button @click=\"onSearch\" type=\"primary\" size=\"small\" style=\"width: 120px;\">查&nbsp;&nbsp;询</el-button>\r\n                </el-form-item>\r\n            </el-form>\r\n            <!-- 表格栏 -->\r\n            <el-table\r\n                ref=\"multipleTable\"\r\n                v-loading=\"listLoading\"\r\n                :data=\"tableData\"\r\n                tooltip-effect=\"dark\"\r\n                style=\"width: 100%\"\r\n                size=\"medium\"\r\n            >\r\n                <el-table-column prop=\"areaStr\" label=\"区域\" align=\"center\" width=\"120\" />\r\n                <el-table-column prop=\"osTypeStr\" label=\"系统类型\" align=\"center\" width=\"120\"/>\r\n                <el-table-column prop=\"channel\" label=\"支付渠道\" align=\"center\" width=\"120\"/>\r\n                <el-table-column prop=\"payTypeStr\" label=\"支付方式\" align=\"center\" width=\"150\"/>\r\n                <el-table-column prop=\"title\" label=\"渠道名称\" align=\"center\" width=\"150\"/>\r\n                <el-table-column prop=\"name\" label=\"渠道名称\" align=\"center\" width=\"150\" />\r\n                <el-table-column prop=\"icon\" label=\"支付平台图标\" align=\"center\" width=\"150\">\r\n                    <template scope=\"scope\">\r\n                        <el-image :fit=\"contain\" style=\"width: 50px; height: 50px\" :src=\"scope.row.icon\" :preview-src-list=\"[scope.row.icon]\"/>\r\n                    </template>\r\n                </el-table-column>\r\n                <el-table-column prop=\"enable\" label=\"是否启用\" align=\"center\" width=\"120\">\r\n                    <template slot-scope=\"scope\">\r\n                        <el-switch v-model=\"scope.row.enable\" disabled/>\r\n                    </template>\r\n                </el-table-column>\r\n                <el-table-column prop=\"discount\" label=\"折扣\" align=\"center\" width=\"120\" />\r\n                <el-table-column prop=\"sort\" label=\"排序\" align=\"center\" width=\"120\" />\r\n                <el-table-column prop=\"note\" label=\"备注\" align=\"center\" width=\"320\" />\r\n                <el-table-column label=\"操作\" align=\"center\" width=\"180\" fixed=\"right\">\r\n                    <template slot-scope=\"scope\">\r\n                        <el-button type=\"text\" @click=\"toDialog('addPay',scope.row)\">更新</el-button>\r\n                        <el-button type=\"text\" @click=\"toDialog('delPay',scope.row)\">删除</el-button>\r\n                    </template>\r\n                </el-table-column>\r\n            </el-table>\r\n            <!-- 分页栏 -->\r\n            <Pagination :total=\"total\" :page.sync=\"search.page.currentPage\" :limit.sync=\"search.page.pageSize\"\r\n                        @pagination=\"fetchData\" @changePageSize=\"changePageSize($event)\"/>\r\n\r\n            <!-- 更新 弹出栏 -->\r\n            <addPay ref=\"addPay\" @fetchData=\"fetchData\"/>\r\n\r\n            <!-- 删除 弹出栏 -->\r\n            <delPay ref=\"delPay\" @fetchData=\"fetchData\"/>\r\n\r\n        </el-card>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n\r\nimport Pagination from '../../../components/Pagination'\r\nimport imageShow from '../../../components/ImageShow/image-show'\r\nimport addPay from './dialog/addPay'\r\nimport delPay from './dialog/delPay'\r\nimport {getAreaList, getArrName, getPayType, getOsType} from \"@/utils/common\";\r\n\r\nexport default {\r\n    components: { Pagination, imageShow, addPay, delPay },\r\n    data() {\r\n        return {\r\n            listLoading: true,\r\n            search: {\r\n                areaId: undefined,\r\n                payType: undefined,\r\n                page: {\r\n                    currentPage: 1,\r\n                    pageSize: 10\r\n                }\r\n            },\r\n            total: 0,\r\n            isCollapse: true,\r\n            areaList: getAreaList(),\r\n            payTypeList: getPayType()\r\n        }\r\n    },\r\n    created() {\r\n        this.fetchData()\r\n    },\r\n    methods: {\r\n        fetchData() {\r\n            const $this = this\r\n            this.listLoading = true\r\n            this.$service.config.getPayChannelList(this.search, function (result){\r\n                const list = result.getChannelsList()\r\n                const data = []\r\n                list.forEach((item, index) => {\r\n                    const json = {\r\n                        \"id\" : item.getId(),\r\n                        \"areaId\" : item.getAreaId(),\r\n                        \"areaStr\" : getArrName($this.areaList, item.getAreaId()),\r\n                        \"osType\" : item.getOsType(),\r\n                        \"osTypeStr\" : getOsType(item.getOsType()),\r\n                        \"channel\" : item.getChannel(),\r\n                        \"payType\" : item.getPayType(),\r\n                        \"payTypeStr\" : getPayType(item.getPayType()),\r\n                        \"title\" : item.getTitle(),\r\n                        \"name\" : item.getName(),\r\n                        \"icon\" : item.getIcon(),\r\n                        \"enable\" : item.getEnable(),\r\n                        \"discount\" : item.getDiscount(),\r\n                        \"sort\" : item.getSort(),\r\n                        \"note\" : item.getNote(),\r\n                        \"struct\" : item\r\n                    }\r\n                    data.push(json)\r\n                })\r\n                $this.total = list.length\r\n                $this.tableData = data\r\n                $this.listLoading = false\r\n            });\r\n        },\r\n        onSearch() {\r\n            this.search.page.currentPage = 1\r\n            this.fetchData()\r\n        },\r\n        changePageSize(msg){\r\n            this.search.page.pageSize = msg.limit\r\n        },\r\n        toDialog(component, row){\r\n            this.$refs[component].dialogVisible = true\r\n            this.$nextTick(()=>{\r\n                this.$refs[component].init(row);\r\n            })\r\n        },\r\n        resetForm() {\r\n            this.$refs.searchForm.resetFields()\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\">\r\n.table-classic-wrapper {\r\n    .el-card {\r\n        min-height: 656px;\r\n    }\r\n\r\n    .control-btns {\r\n        margin-bottom: 20px;\r\n    }\r\n\r\n    .search-form {\r\n        padding-top: 18px;\r\n        margin-bottom: 15px;\r\n        background-color: #f7f8fb;\r\n    }\r\n\r\n    .el-table thead {\r\n        font-weight: 600;\r\n\r\n        th {\r\n            background-color: #f2f3f7;\r\n        }\r\n    }\r\n\r\n    .dialog-form {\r\n        .el-input {\r\n            width: 380px;\r\n        }\r\n\r\n        .footer-item {\r\n            margin-top: 50px;\r\n            text-align: right;\r\n        }\r\n    }\r\n    .el-divider--horizontal{\r\n        width: 95%;\r\n        margin-left: 20px;\r\n        margin-right: 20px;\r\n    }\r\n    .search-box {\r\n        margin-left: 0;\r\n        text-align: center;\r\n        margin-top: 10px;\r\n        margin-bottom: 20px\r\n    }\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/manage/config"}]}