ACA,YAAA,IAAA,GAAA,MAAA,CAAA,aAAA,EAAA;AACA,YAAA,IAAA,GAAA,EAAA;AACA,QAAA,IAAA,CAAA,OAAA,CAAA,UAAA,IAAA,EAAA,KAAA,EAAA;AACA,cAAA,IAAA,GAAA;AACA,kBAAA,IAAA,CAAA,KAAA,EADA;AAEA,sBAAA,IAAA,CAAA,SAAA,EAFA;AAGA,uBAAA,UAAA,CAAA,KAAA,CAAA,QAAA,EAAA,IAAA,CAAA,SAAA,EAAA,CAHA;AAIA,oBAAA,IAAA,CAAA,OAAA,EAJA;AAKA,0BAAA,IAAA,CAAA,aAAA,EALA;AAMA,2BAAA,IAAA,CAAA,cAAA,EANA;AAOA,sBAAA,IAAA,CAAA,SAAA,EAPA;AAQA,oBAAA,IAAA,CAAA,OAAA,EARA;AASA,yBAAA,IAAA,IAAA,CAAA,IAAA,CAAA,YAAA,KAAA,IAAA,EAAA,MAAA,CAAA,qBAAA,CATA;AAUA,sBAAA;AAVA,WAAA;AAYA,UAAA,IAAA,CAAA,IAAA,CAAA,IAAA;AACA,SAdA;AAeA,QAAA,KAAA,CAAA,KAAA,GAAA,IAAA,CAAA,MAAA;AACA,QAAA,KAAA,CAAA,SAAA,GAAA,IAAA;AACA,QAAA,KAAA,CAAA,WAAA,GAAA,KAAA;AACA,OArBA;AAsBA,KA3BA;AA4BA,IAAA,QA5BA,sBA4BA;AACA,WAAA,MAAA,CAAA,IAAA,CAAA,WAAA,GAAA,CAAA;AACA,WAAA,SAAA;AACA,KA/BA;AAgCA,IAAA,QAhCA,oBAgCA,SAhCA,EAgCA,GAhCA,EAgCA;AAAA;;AACA,WAAA,KAAA,CAAA,SAAA,EAAA,aAAA,GAAA,IAAA;AACA,WAAA,SAAA,CAAA,YAAA;AACA,QAAA,KAAA,CAAA,KAAA,CAAA,SAAA,EAAA,IAAA,CAAA,GAAA;AACA,OAFA;AAGA,KArCA;AAsCA,IAAA,SAtCA,uBAsCA;AACA,WAAA,KAAA,CAAA,UAAA,CAAA,WAAA;AACA;AAxCA;AA3BA,CAAA","sourcesContent":["<template>\r\n    <div class=\"table-classic-wrapper\">\r\n        <el-card shadow=\"always\">\r\n            <!-- 查询栏 -->\r\n            <el-form\r\n                ref=\"searchForm\"\r\n                :inline=\"true\"\r\n                :model=\"search\"\r\n                label-width=\"90px\"\r\n                class=\"search-form\"\r\n            >\r\n                <el-form-item label=\"工会ID\">\r\n                    <el-input v-model=\"search.id\" placeholder=\"工会ID\"/>\r\n                </el-form-item>\r\n                <el-form-item label=\"区域\">\r\n                    <el-select v-model=\"search.areaId\" placeholder=\"请选择\">\r\n                        <el-option v-for=\"item in areaData\"\r\n                                   :key=\"item.value\"\r\n                                   :label=\"item.label\"\r\n                                   :value=\"item.value\">\r\n                        </el-option>\r\n                    </el-select>\r\n                </el-form-item>\r\n                <el-form-item label=\"是否启用\">\r\n                    <el-switch v-model=\"search.enable\"/>\r\n                </el-form-item>\r\n                <el-form-item style=\"padding-left: 20px\">\r\n                    <el-button @click=\"onSearch\" type=\"primary\" size=\"small\" style=\"width: 120px;\">查&nbsp;&nbsp;询</el-button>\r\n                </el-form-item>\r\n            </el-form>\r\n            <!-- 操作栏 -->\r\n            <div class=\"control-btns\">\r\n                <el-button type=\"primary\" @click=\"toDialog('edit', '')\">+ 新增工会</el-button>\r\n            </div>\r\n            <!-- 表格栏 -->\r\n            <el-table\r\n                ref=\"multipleTable\"\r\n                v-loading=\"listLoading\"\r\n                :data=\"tableData\"\r\n                tooltip-effect=\"dark\"\r\n                style=\"width: 100%\"\r\n                size=\"medium\"\r\n            >\r\n                <el-table-column prop=\"id\" label=\"工会ID\" align=\"center\" width=\"70\" />\r\n                <el-table-column prop=\"name\" label=\"工会名称\" align=\"center\" width=\"250\" />\r\n                <el-table-column prop=\"areaStr\" label=\"区域\" align=\"center\" width=\"120\" />\r\n                <el-table-column prop=\"anchorCount\" label=\"主播数\" align=\"center\" width=\"120\"/>\r\n                <el-table-column prop=\"enable\" label=\"状态\" align=\"center\" width=\"120\">\r\n                    <template scope=\"scope\">\r\n                        <div slot=\"reference\">\r\n                            <el-switch v-model=\"scope.row.enable\" disabled/>\r\n                        </div>\r\n                    </template>\r\n                </el-table-column>\r\n                <el-table-column prop=\"createdAt\" label=\"创建时间\" align=\"center\" width=\"250\"/>\r\n                <el-table-column prop=\"note\" label=\"备注\" align=\"center\" width=\"220\"/>\r\n                <el-table-column label=\"操作\" align=\"center\">\r\n                    <template slot-scope=\"scope\">\r\n                        <el-button type=\"text\" @click=\"toDialog('edit', scope.row)\">编辑</el-button>\r\n                        <el-button type=\"text\" @click=\"toDialog('close', scope.row)\">关闭</el-button>\r\n                    </template>\r\n                </el-table-column>\r\n            </el-table>\r\n            <!-- 分页栏 -->\r\n            <Pagination :total=\"total\" :page.sync=\"search.page.currentPage\" :limit.sync=\"search.page.pageSize\"\r\n                        @pagination=\"fetchData\"/>\r\n\r\n            <!-- 新增或编辑 -->\r\n            <edit ref=\"edit\" @fetchData=\"fetchData\"/>\r\n\r\n            <!-- 关闭 -->\r\n            <close ref=\"close\"/>\r\n        </el-card>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport Pagination from '../../../components/Pagination'\r\nimport edit from './dialog/edit'\r\nimport close from './dialog/close'\r\nimport {getAreaList, getBool, getArrName} from \"@/utils/common\";\r\n\r\n\r\n\r\nexport default {\r\n    components: { Pagination, edit, close },\r\n    data() {\r\n        return {\r\n            // 数据列表加载动画\r\n            listLoading: true,\r\n            // 查询列表参数对象\r\n            search: {\r\n                id: undefined,\r\n                areaData: undefined,\r\n                enable: true,\r\n                page: {\r\n                    currentPage: 1,\r\n                    pageSize: 10\r\n                }\r\n            },\r\n            // 数据总条数\r\n            total: 0,\r\n            // 防止多次连续提交表单\r\n            isSubmit: false,\r\n            areaData : getAreaList(),\r\n            boolDict : getBool()\r\n        }\r\n    },\r\n    created() {\r\n        this.fetchData()\r\n    },\r\n    methods: {\r\n        // 获取数据列表\r\n        fetchData() {\r\n            const $this = this\r\n            this.listLoading = true\r\n            this.$service.guild.getGuildList(this.search, function (result){\r\n                const list = result.getGuildsList();\r\n                const data = []\r\n                list.forEach((item, index)=>{\r\n                    const json = {\r\n                        \"id\" : item.getId(),\r\n                        \"areaId\" : item.getAreaId(),\r\n                        \"areaStr\" : getArrName($this.areaData, item.getAreaId()),\r\n                        \"name\" : item.getName(),\r\n                        \"inviteCode\" : item.getInviteCode(),\r\n                        \"anchorCount\" : item.getAnchorCount(),\r\n                        \"enable\" : item.getEnable(),\r\n                        \"note\" : item.getNote(),\r\n                        \"createdAt\" : new Date(item.getCreatedAt() * 1000).format('yyyy-MM-dd hh:mm:ss'),\r\n                        \"struct\" : item\r\n                    }\r\n                    data.push(json)\r\n                })\r\n                $this.total = list.length\r\n                $this.tableData = data\r\n                $this.listLoading = false\r\n            })\r\n        },\r\n        onSearch() {\r\n            this.search.page.currentPage = 1\r\n            this.fetchData()\r\n        },\r\n        toDialog(component, row){\r\n            this.$refs[component].dialogVisible = true\r\n            this.$nextTick(()=>{\r\n                this.$refs[component].init(row);\r\n            })\r\n        },\r\n        resetForm() {\r\n            this.$refs.searchForm.resetFields()\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\">\r\n.table-classic-wrapper {\r\n    .el-card {\r\n        min-height: 656px;\r\n    }\r\n\r\n    .control-btns {\r\n        margin-bottom: 20px;\r\n    }\r\n\r\n    .search-form {\r\n        padding-top: 18px;\r\n        margin-bottom: 15px;\r\n        background-color: #f7f8fb;\r\n    }\r\n\r\n    .el-table thead {\r\n        font-weight: 600;\r\n\r\n        th {\r\n            background-color: #f2f3f7;\r\n        }\r\n    }\r\n\r\n    .dialog-form {\r\n        .el-input {\r\n            width: 380px;\r\n        }\r\n        .footer-item {\r\n            margin-top: 50px;\r\n            text-align: right;\r\n        }\r\n    }\r\n    .el-divider--horizontal{\r\n        width: 95%;\r\n        margin-left: 20px;\r\n        margin-right: 20px;\r\n    }\r\n    .search-box {\r\n        margin-left: 0;\r\n        text-align: center;\r\n        margin-top: 10px;\r\n        margin-bottom: 20px\r\n    }\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/manage/union"}]}