DA,gBACA,GADA,EACA;AACA,WAAA,MAAA,CAAA,QAAA,GAAA,GAAA,CAAA,OAAA;AACA,WAAA,SAAA;AACA,KAJA;AAKA,IAAA,SALA,uBAKA;AACA,UAAA,KAAA,GAAA,IAAA;AACA,WAAA,WAAA,GAAA,IAAA;AACA,WAAA,QAAA,CAAA,KAAA,CAAA,WAAA,CAAA,KAAA,MAAA,EAAA,UAAA,MAAA,EAAA;AACA,YAAA,IAAA,GAAA,MAAA,CAAA,cAAA,EAAA;AACA,YAAA,IAAA,GAAA,EAAA;AACA,QAAA,IAAA,CAAA,OAAA,CAAA,UAAA,IAAA,EAAA,KAAA,EAAA;AACA,cAAA,IAAA,GAAA;AACA,mBAAA,UAAA,CAAA,KAAA,CAAA,OAAA,EAAA,IAAA,CAAA,QAAA,EAAA,CADA;AAEA,uBAAA,UAAA,CAAA,KAAA,CAAA,QAAA,EAAA,IAAA,CAAA,SAAA,EAAA,CAFA;AAGA,oBAAA,UAAA,CAAA,KAAA,CAAA,YAAA,EAAA,IAAA,CAAA,OAAA,EAAA,CAHA;AAIA,uBAAA,IAAA,CAAA,UAAA,EAJA;AAKA,qBAAA,IAAA,CAAA,QAAA,EALA;AAMA,mBAAA,IAAA,CAAA,MAAA;AANA,WAAA;AAQA,UAAA,IAAA,CAAA,IAAA,CAAA,IAAA;AACA,SAVA;AAWA,QAAA,KAAA,CAAA,KAAA,GAAA,MAAA,CAAA,aAAA,EAAA;AACA,QAAA,KAAA,CAAA,SAAA,GAAA,IAAA;AACA,QAAA,KAAA,CAAA,WAAA,GAAA,KAAA;AACA,OAjBA;AAkBA,KA1BA;AA2BA,IAAA,IA3BA,gBA2BA,GA3BA,EA2BA;AAAA;;AACA,WAAA,WAAA,GAAA,IAAA;AACA,UAAA,GAAA,GAAA,GAAA,CAAA,GAAA;AACA,WAAA,SAAA,CAAA,YAAA;AACA,QAAA,KAAA,CAAA,KAAA,CAAA,aAAA,CAAA,OAAA,CAAA,GAAA;AACA,OAFA;AAGA,KAjCA;AAkCA,IAAA,UAlCA,wBAkCA;AAAA;;AACA,WAAA,WAAA,GAAA,KAAA;AACA,WAAA,SAAA,CAAA,YAAA;AACA,QAAA,MAAA,CAAA,KAAA,CAAA,aAAA,CAAA,QAAA;AACA,OAFA;AAGA,KAvCA;AAwCA,IAAA,WAxCA,yBAwCA;AACA,WAAA,aAAA,GAAA,KAAA;AACA,WAAA,SAAA;AACA,WAAA,KAAA,CAAA,WAAA;AACA;AA5CA;AA7BA,CAAA","sourcesContent":["<template>\r\n    <el-dialog title=\"查看视频\" :visible.sync=\"dialogVisible\" append-to-body width=\"40%\" :before-close=\"handleClose\">\r\n        <div class=\"table-classic-wrapper dialog-list\">\r\n            <!-- 表格栏 -->\r\n            <el-table\r\n                ref=\"multipleTable\"\r\n                v-loading=\"listLoading\"\r\n                :data=\"tableData\"\r\n                tooltip-effect=\"dark\"\r\n                style=\"width: 100%\"\r\n                size=\"medium\"\r\n            >\r\n                <el-table-column prop=\"thumb\" label=\"视频\" align=\"center\">\r\n                    <template scope=\"scope\">\r\n                        <el-image @click=\"play(scope.row)\" style=\"width: 50px; height: 50px\" :src=\"scope.row.thumb\" contain></el-image>\r\n                    </template>\r\n                </el-table-column>\r\n                <el-table-column prop=\"thumb\" label=\"应用App\" align=\"center\" width=\"120\">\r\n                    <template scope=\"scope\">\r\n                        <div slot=\"reference\">\r\n                            {{ scope.row.app.label }}\r\n                            <span v-if=\"scope.row.app.os === 1\">\r\n                                <i class=\"icon-android-fill\"></i>\r\n                            </span>\r\n                            <span v-else>\r\n                                <i class=\"icon-pingguo\"></i>\r\n                            </span>\r\n                        </div>\r\n                    </template>\r\n                </el-table-column>\r\n                <el-table-column prop=\"areaStr\" label=\"区域\" align=\"center\" width=\"120\"/>\r\n                <el-table-column prop=\"type\" label=\"文件类型\" align=\"center\" width=\"120\"/>\r\n                <el-table-column prop=\"ownerId\" label=\"所有者ID\" align=\"center\" width=\"120\"/>\r\n                <!--                    <el-table-column label=\"操作\" align=\"center\">-->\r\n                <!--                        <template slot-scope=\"scope\">-->\r\n                <!--                            <el-button type=\"success\" plain size=\"mini\" @click=\"handleRefuse(scope.$index, scope.row)\">删除</el-button>-->\r\n                <!--                        </template>-->\r\n                <!--                    </el-table-column>-->\r\n            </el-table>\r\n            <!-- 分页栏 -->\r\n            <Pagination :total=\"total\" :page.sync=\"search.page.currentPage\" :limit.sync=\"search.page.pageSize\"\r\n                        @pagination=\"fetchData\"/>\r\n        </div>\r\n        <el-dialog\r\n            title=\"播放视频\"\r\n            :visible.sync=\"playVisible\"\r\n            :before-close=\"closeVideo\"\r\n            :append-to-body=\"true\">\r\n            <div class=\"content-item\">\r\n                <VueVideoPlayer ref=\"myVideoPlayer\"></VueVideoPlayer>\r\n            </div>\r\n        </el-dialog>\r\n    </el-dialog>\r\n</template>\r\n\r\n<script>\r\nimport Pagination from '../../../../components/Pagination'\r\nimport VueVideoPlayer from '../../../../components/VueVideoPlayer'\r\nimport {getAppList, getAppName, getAreaList, getArrName, getFileType} from \"@/utils/dist\"\r\nimport {toTime} from \"@/utils/date\";\r\n\r\nexport default {\r\n    name: 'Table',\r\n    components: { Pagination, VueVideoPlayer },\r\n    data() {\r\n        return {\r\n            // 数据列表加载动画\r\n            listLoading: true,\r\n            search: {\r\n                anchorId : undefined,\r\n                status: 5,\r\n                page: {\r\n                    currentPage: 1,\r\n                    pageSize: 10\r\n                }\r\n            },\r\n            videoList: [],\r\n            imgList: [],\r\n            // 数据总条数\r\n            total: 0,\r\n            playVisible: false,\r\n            dialogVisible: false,\r\n            appList: getAppList(),\r\n            areaList: getAreaList(true),\r\n            fileTypeList: getFileType()\r\n        }\r\n    },\r\n    created() {\r\n        this.fetchData()\r\n    },\r\n    methods: {\r\n        init(row){\r\n            this.search.anchorId = row.ownerId\r\n            this.fetchData()\r\n        },\r\n        fetchData() {\r\n            const $this = this\r\n            this.listLoading = true\r\n            this.$service.audit.getLiveList(this.search, function (result){\r\n                const list = result.getRecordsList()\r\n                const data = []\r\n                list.forEach((item, index)=>{\r\n                    const json = {\r\n                        \"app\" : getAppName($this.appList, item.getAppId()),\r\n                        \"areaStr\" : getArrName($this.areaList, item.getAreaId()),\r\n                        \"type\" : getArrName($this.fileTypeList, item.getType()),\r\n                        \"ownerId\" : item.getOwnerId(),\r\n                        \"thumb\" : item.getThumb(),\r\n                        \"uri\" : item.getUri()\r\n                    }\r\n                    data.push(json)\r\n                })\r\n                $this.total = result.getTotalCount()\r\n                $this.tableData = data\r\n                $this.listLoading = false\r\n            });\r\n        },\r\n        play(row) {\r\n            this.playVisible = true;\r\n            let src = row.uri\r\n            this.$nextTick(()=>{\r\n                this.$refs.myVideoPlayer.initSrc(src);\r\n            })\r\n        },\r\n        closeVideo(){\r\n            this.playVisible = false;\r\n            this.$nextTick(()=>{\r\n                this.$refs.myVideoPlayer.emptySrc();\r\n            })\r\n        },\r\n        closeDialog() {\r\n            this.dialogVisible = false\r\n            this.resetForm()\r\n            this.$emit('fetchData');\r\n        },\r\n    }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\">\r\n.dialog-list{\r\n    padding-left: 20px;\r\n    padding-right: 20px;\r\n    padding-bottom:  20px;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/manage/audit/dialog"}]}