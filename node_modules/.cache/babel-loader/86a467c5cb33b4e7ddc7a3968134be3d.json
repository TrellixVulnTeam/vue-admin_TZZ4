CAAA,UAAA,GAAA,EAAA;AACA,YAAA,IAAA,GAAA,GAAA,CAAA,IAAA;;AACA,YAAA,IAAA,CAAA,IAAA,KAAA,CAAA,EAAA;AACA,UAAA,KAAA,CAAA,KAAA,GAAA,IAAA,CAAA,IAAA,CAAA,KAAA;AACA,UAAA,KAAA,CAAA,SAAA,GAAA,IAAA,CAAA,IAAA,CAAA,IAAA;AACA,UAAA,KAAA,CAAA,WAAA,GAAA,KAAA;AACA;AACA,OAPA,EAOA,KAPA,CAOA,YAAA;AACA,QAAA,KAAA,CAAA,WAAA,GAAA,KAAA;AACA,OATA;AAUA,KApBA;AAqBA;AACA,IAAA,QAtBA,sBAsBA;AACA,WAAA,MAAA,CAAA,WAAA,GAAA,CAAA;AACA,WAAA,SAAA;AACA,KAzBA;AA0BA;AACA,IAAA,QA3BA,oBA2BA,SA3BA,EA2BA,GA3BA,EA2BA;AAAA;;AACA,WAAA,KAAA,CAAA,SAAA,EAAA,aAAA,GAAA,IAAA;AACA,WAAA,SAAA,CAAA,YAAA;AACA,QAAA,MAAA,CAAA,KAAA,CAAA,SAAA,EAAA,IAAA,CAAA,GAAA;AACA,OAFA;AAGA,KAhCA;AAiCA;AACA,IAAA,SAlCA,uBAkCA;AACA,WAAA,MAAA,GAAA,KAAA,SAAA,EAAA;AACA,KApCA;AAqCA;AACA,IAAA,YAtCA,wBAsCA,KAtCA,EAsCA,GAtCA,EAsCA;AAAA;;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA,EAAA,GAAA;AACA,WAAA,QAAA,CAAA,mBAAA,EAAA,IAAA,EAAA;AACA,QAAA,iBAAA,EAAA,IADA;AAEA,QAAA,gBAAA,EAAA,IAFA;AAGA,QAAA,IAAA,EAAA;AAHA,OAAA,EAIA,IAJA,CAIA,YAAA;AACA;AACA;AACA,QAAA,MAAA,CAAA,QAAA,CAAA;AACA,UAAA,IAAA,EAAA,SADA;AAEA,UAAA,OAAA,EAAA;AAFA,SAAA;AAIA,OAXA,EAWA,KAXA,CAWA,YAAA;AACA,QAAA,MAAA,CAAA,QAAA,CAAA;AACA,UAAA,IAAA,EAAA,MADA;AAEA,UAAA,OAAA,EAAA;AAFA,SAAA;AAIA,OAhBA;AAiBA,KAzDA;AA0DA;AACA,IAAA,WA3DA,yBA2DA;AACA,UAAA,KAAA,iBAAA,CAAA,MAAA,GAAA,CAAA,EAAA;AACA,aAAA,QAAA,CAAA;AACA,UAAA,OAAA,EAAA,aADA;AAEA,UAAA,IAAA,EAAA;AAFA,SAAA;AAIA,OALA,MAKA;AACA,aAAA,YAAA;AACA;AACA,KApEA;AAqEA,IAAA,SArEA,uBAqEA;AACA,aAAA;AACA,QAAA,GAAA,EAAA,SADA;AAEA,QAAA,GAAA,EAAA,SAFA;AAGA,QAAA,OAAA,EAAA,SAHA;AAIA,QAAA,WAAA,EAAA,CAJA;AAKA,QAAA,QAAA,EAAA;AALA,OAAA;AAOA;AA7EA;AArBA,CAAA","sourcesContent":["<template>\n    <div class=\"table-classic-wrapper\">\n        <el-card shadow=\"always\">\n            <!-- 操作栏 -->\n            <div class=\"control-btns\">\n                <el-button type=\"primary\" @click=\"toDialog('addWords', '')\">+ 新增话术</el-button>\n                <el-button type=\"danger\" @click=\"batchDelete\">批量删除</el-button>\n            </div>\n            <!-- 查询栏 -->\n            <el-form\n                ref=\"searchForm\"\n                :inline=\"true\"\n                :model=\"search\"\n                label-width=\"90px\"\n                class=\"search-form\"\n            >\n                <template>\n                    <el-row>\n                        <el-col :span=\"6\">\n                            <el-form-item label=\"状态\" >\n                                <el-select v-model=\"search.area\" placeholder=\"请选择\">\n                                    <el-option v-for=\"item in areaData\"\n                                               :key=\"item.value\"\n                                               :label=\"item.label\"\n                                               :value=\"item.value\">\n                                    </el-option>\n                                </el-select>\n                            </el-form-item>\n                        </el-col>\n                        <el-col :span=\"6\">\n                            <el-form-item label=\"场景标签\">\n                                <el-select v-model=\"search.tags\" placeholder=\"请选择\">\n                                    <el-option v-for=\"item in areaData\"\n                                               :key=\"item.value\"\n                                               :label=\"item.label\"\n                                               :value=\"item.value\">\n                                    </el-option>\n                                </el-select>\n                            </el-form-item>\n                        </el-col>\n                        <el-col :span=\"6\">\n                            <el-form-item>\n                                <el-button type=\"primary\" @click=\"onSubmit\">筛选</el-button>\n                                <el-button type=\"danger\" @click=\"onSubmit\">重置</el-button>\n                            </el-form-item>\n                        </el-col>\n                    </el-row>\n                </template>\n            </el-form>\n            <!-- 表格栏 -->\n            <el-table\n                ref=\"multipleTable\"\n                v-loading=\"listLoading\"\n                :data=\"tableData\"\n                tooltip-effect=\"dark\"\n                style=\"width: 100%\"\n                size=\"medium\"\n                @selection-change=\"handleSelectionChange\"\n            >\n                <el-table-column type=\"selection\" width=\"60\"/>\n                <el-table-column prop=\"wordsId\" label=\"话术ID\" align=\"center\" width=\"150\"/>\n                <el-table-column prop=\"area\" label=\"区域\" align=\"center\" width=\"150\">\n                    <template slot-scope=\"scope\">\n                        <div slot=\"reference\">\n                            <el-tag size=\"medium\">{{ scope.row.area }}</el-tag>\n                        </div>\n                    </template>\n                </el-table-column>\n                <el-table-column prop=\"tags\" label=\"标签\" align=\"center\" width=\"150\">\n                    <template slot-scope=\"scope\">\n                        <div slot=\"reference\">\n                            <el-tag size=\"medium\">{{ scope.row.tags }}</el-tag>\n                        </div>\n                    </template>\n                </el-table-column>\n                <el-table-column prop=\"status\" label=\"状态\" align=\"center\" width=\"150\">\n                    <template slot-scope=\"scope\">\n                        <el-switch v-model=\"scope.row.status\"/>\n                    </template>\n                </el-table-column>\n                <el-table-column prop=\"content\" label=\"内容\" align=\"center\"/>\n            </el-table>\n            <!-- 分页栏 -->\n            <Pagination :total=\"total\" :page.sync=\"search.currentPage\" :limit.sync=\"search.pageSize\"\n                        @pagination=\"fetchData\"/>\n\n            <!-- 编辑资料 -->\n            <addWords ref=\"addWords\"/>\n        </el-card>\n    </div>\n</template>\n\n<script>\nimport { getTableList } from '../../../api/api'\nimport Pagination from '../../../components/Pagination'\nimport addWords from './dialog/add-robot-message'\nimport { areaData, apps } from '@/dict/index'\n\nexport default {\n    components: { Pagination, apps, addWords},\n    data() {\n        return {\n            // 数据列表加载动画\n            listLoading: true,\n            // 查询列表参数对象\n            search: this.initQuery(),\n            // 数据总条数\n            total: 0,\n            // 多选数据暂存数组\n            multipleSelection: [],\n            // 防止多次连续提交表单\n            isSubmit: false,\n            areaData,\n            apps\n        }\n    },\n    created() {\n        this.fetchData()\n    },\n    methods: {\n        // 多选操作\n        handleSelectionChange(val) {\n            this.multipleSelection = val\n        },\n        // 获取数据列表\n        fetchData() {\n            this.listLoading = true\n            let url = process.env.VUE_APP_JSON_URI + \"/words-manage.json\"\n            // 获取数据列表接口\n            getTableList(this.search, url).then(res => {\n                const data = res.data\n                if (data.code === 0) {\n                    this.total = data.data.total\n                    this.tableData = data.data.list\n                    this.listLoading = false\n                }\n            }).catch(() => {\n                this.listLoading = false\n            })\n        },\n        // 查询数据\n        onSearch() {\n            this.search.currentPage = 1\n            this.fetchData()\n        },\n        // 弹框\n        toDialog(component, row){\n            this.$refs[component].dialogVisible = true\n            this.$nextTick(()=>{\n                this.$refs[component].init(row)\n            })\n        },\n        //重置\n        resetForm() {\n            this.search = this.initQuery();\n        },\n        // 删除数据\n        handleDelete(index, row) {\n            console.log(index, row)\n            this.$confirm('此操作将删除选中数据, 是否继续?', '提示', {\n                confirmButtonText: '确定',\n                cancelButtonText: '取消',\n                type: 'warning'\n            }).then(() => {\n                // 此处可添加--删除接口\n                // 删除成功调用fetchData方法更新列表\n                this.$message({\n                    type: 'success',\n                    message: '删除成功!'\n                })\n            }).catch(() => {\n                this.$message({\n                    type: 'info',\n                    message: '已取消删除'\n                })\n            })\n        },\n        // 批量删除\n        batchDelete() {\n            if (this.multipleSelection.length < 1) {\n                this.$message({\n                    message: '请先选择要删除的数据！',\n                    type: 'warning'\n                })\n            } else {\n                this.handleDelete()\n            }\n        },\n        initQuery() {\n            return {\n                uid: undefined,\n                app: undefined,\n                robotId: undefined,\n                currentPage: 1,\n                pageSize: 10\n            }\n        }\n    }\n}\n</script>\n\n<style lang=\"less\">\n.table-classic-wrapper {\n    .el-card {\n        min-height: 656px;\n    }\n\n    .control-btns {\n        margin-bottom: 20px;\n    }\n\n    .search-form {\n        padding-top: 18px;\n        margin-bottom: 15px;\n        background-color: #f7f8fb;\n    }\n\n    .el-table thead {\n        font-weight: 600;\n\n        th {\n            background-color: #f2f3f7;\n        }\n    }\n\n    .dialog-form {\n        .el-input {\n            width: 380px;\n        }\n        .footer-item {\n            margin-top: 50px;\n            text-align: right;\n        }\n    }\n    .el-divider--horizontal{\n        width: 95%;\n        margin-left: 20px;\n        margin-right: 20px;\n    }\n    .search-box {\n        margin-left: 0;\n        text-align: center;\n        margin-top: 10px;\n        margin-bottom: 20px\n    }\n}\n</style>\n"],"sourceRoot":"src/views/manage/aia"}]}