EAMA,GANA,EAMA;AAAA;;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA,EAAA,GAAA;AACA,WAAA,QAAA,CAAA,mBAAA,EAAA,IAAA,EAAA;AACA,QAAA,iBAAA,EAAA,IADA;AAEA,QAAA,gBAAA,EAAA,IAFA;AAGA,QAAA,IAAA,EAAA;AAHA,OAAA,EAIA,IAJA,CAIA,YAAA;AACA;AACA;AACA,QAAA,KAAA,CAAA,QAAA,CAAA;AACA,UAAA,IAAA,EAAA,SADA;AAEA,UAAA,OAAA,EAAA;AAFA,SAAA;AAIA,OAXA,EAWA,KAXA,CAWA,YAAA;AACA,QAAA,KAAA,CAAA,QAAA,CAAA;AACA,UAAA,IAAA,EAAA,MADA;AAEA,UAAA,OAAA,EAAA;AAFA,SAAA;AAIA,OAhBA;AAiBA,KAzBA;AA0BA;AACA,IAAA,WA3BA,yBA2BA;AACA,UAAA,KAAA,iBAAA,CAAA,MAAA,GAAA,CAAA,EAAA;AACA,aAAA,QAAA,CAAA;AACA,UAAA,OAAA,EAAA,aADA;AAEA,UAAA,IAAA,EAAA;AAFA,SAAA;AAIA,OALA,MAKA;AACA,aAAA,YAAA;AACA;AACA,KApCA;AAqCA;AACA,IAAA,SAtCA,uBAsCA;AAAA;;AACA,WAAA,WAAA,GAAA,IAAA;AACA,UAAA,GAAA,GAAA,4CAAA,CAFA,CAGA;;AACA,6BAAA,KAAA,SAAA,EAAA,GAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,YAAA,IAAA,GAAA,GAAA,CAAA,IAAA;;AACA,YAAA,IAAA,CAAA,IAAA,KAAA,CAAA,EAAA;AACA,UAAA,MAAA,CAAA,KAAA,GAAA,IAAA,CAAA,IAAA,CAAA,KAAA;AACA,UAAA,MAAA,CAAA,SAAA,GAAA,IAAA,CAAA,IAAA,CAAA,IAAA;AACA,UAAA,MAAA,CAAA,WAAA,GAAA,KAAA;AACA;AACA,OAPA,EAOA,KAPA,CAOA,YAAA;AACA,QAAA,MAAA,CAAA,WAAA,GAAA,KAAA;AACA,OATA;AAUA,KApDA;AAqDA;AACA,IAAA,QAtDA,sBAsDA;AACA,WAAA,SAAA,CAAA,WAAA,GAAA,CAAA;AACA,WAAA,SAAA;AACA,KAzDA;AA0DA;AACA,IAAA,QA3DA,oBA2DA,SA3DA,EA2DA,GA3DA,EA2DA;AAAA;;AACA,WAAA,KAAA,CAAA,SAAA,EAAA,aAAA,GAAA,IAAA;AACA,WAAA,SAAA,CAAA,YAAA;AACA,QAAA,MAAA,CAAA,KAAA,CAAA,SAAA,EAAA,IAAA,CAAA,GAAA;AACA,OAFA;AAGA,KAhEA;AAiEA;AACA,IAAA,SAlEA,uBAkEA;AACA,WAAA,SAAA,GAAA,KAAA,SAAA,EAAA;AACA,KApEA;AAqEA,IAAA,SArEA,uBAqEA;AACA,aAAA;AACA,QAAA,GAAA,EAAA,SADA;AAEA,QAAA,GAAA,EAAA,SAFA;AAGA,QAAA,OAAA,EAAA,SAHA;AAIA,QAAA,WAAA,EAAA,CAJA;AAKA,QAAA,QAAA,EAAA;AALA,OAAA;AAOA;AA7EA;AA9BA,C","sourcesContent":["<template>\n    <div class=\"table-classic-wrapper\">\n        <Hints>\n            <template slot=\"hintName\"> 编辑 < {{ robotName }} > 话术</template>\n        </Hints>\n        <el-card shadow=\"always\">\n            <!-- 操作栏 -->\n            <div class=\"control-btns\">\n                <el-button type=\"primary\" @click=\"toDialog('addWords', '')\">+ 新增话术</el-button>\n                <el-button type=\"danger\" @click=\"batchDelete\">批量删除</el-button>\n            </div>\n            <!-- 表格栏 -->\n            <el-table\n                ref=\"multipleTable\"\n                v-loading=\"listLoading\"\n                :data=\"tableData\"\n                tooltip-effect=\"dark\"\n                style=\"width: 100%\"\n                size=\"medium\"\n                @selection-change=\"handleSelectionChange\"\n            >\n                <el-table-column type=\"selection\" width=\"60\"/>\n                <el-table-column prop=\"robotId\" label=\"机器人ID\" align=\"center\" width=\"150\" />\n                <el-table-column prop=\"photo\" label=\"头像\" align=\"center\" width=\"150\">\n                    <template scope=\"scope\">\n                        <el-image style=\"width: 50px; height: 50px\" :src=\"scope.row.photo\" contain></el-image>\n                    </template>\n                </el-table-column>\n                <el-table-column prop=\"type\" label=\"类型\" align=\"center\" width=\"150\"/>\n                <el-table-column prop=\"content\" label=\"内容\" align=\"center\" width=\"350\" />\n                <el-table-column prop=\"url\" label=\"连接\" align=\"center\" width=\"150\">\n                    <template slot-scope=\"scope\">\n                        <a :href=\"scope.row.url\" style=\"color: #1E88C7\">{{ scope.row.fileName }}</a>\n                    </template>\n                </el-table-column>\n                <el-table-column prop=\"url\" label=\"缩略图\" align=\"center\" width=\"150\">\n                    <template slot-scope=\"scope\">\n                        <el-image style=\"width: 50px; height: 50px\" :src=\"scope.row.url\" contain></el-image>\n                    </template>\n                </el-table-column>\n                <el-table-column prop=\"sort\" label=\"排序\" align=\"center\" width=\"150\"/>\n                <el-table-column prop=\"status\" label=\"状态\" align=\"center\">\n                    <template slot-scope=\"scope\">\n                        <el-switch v-model=\"scope.row.status\"/>\n                    </template>\n                </el-table-column>\n            </el-table>\n            <!-- 分页栏 -->\n            <Pagination :total=\"total\" :page.sync=\"listQuery.currentPage\" :limit.sync=\"listQuery.pageSize\"\n                        @pagination=\"fetchData\"/>\n\n            <!-- 新增话术 -->\n            <addWords ref=\"addWords\"/>\n        </el-card>\n    </div>\n</template>\n\n<script>\nimport { getTableList } from '../../../api/api'\nimport Pagination from '../../../components/Pagination'\nimport addWords from './dialog/add-words'\nimport Hints from '../../../components/Hints'\nimport { areaData, apps } from '@/dict/index'\n\nexport default {\n    name: 'Table',\n    components: {Pagination, Hints, apps, addWords},\n    data() {\n        return {\n            // 数据列表加载动画\n            listLoading: true,\n            // 查询列表参数对象\n            listQuery: this.initQuery(),\n            // 数据总条数\n            total: 0,\n            // 表格数据数组\n            tableData: [],\n            // 多选数据暂存数组\n            multipleSelection: [],\n            // 新增/编辑 弹出框显示/隐藏\n            formVisible: false,\n            // 防止多次连续提交表单\n            isSubmit: false,\n            robotId: '',\n            robotName: '',\n            areaData,\n            apps\n        }\n    },\n    created() {\n        this.robotId = this.$route.query.robotId\n        this.robotName = this.$route.query.nickname\n        this.fetchData()\n    },\n    methods: {\n        // 多选操作\n        handleSelectionChange(val) {\n            this.multipleSelection = val\n        },\n        // 删除数据\n        handleDelete(index, row) {\n            console.log(index, row)\n            this.$confirm('此操作将删除选中数据, 是否继续?', '提示', {\n                confirmButtonText: '确定',\n                cancelButtonText: '取消',\n                type: 'warning'\n            }).then(() => {\n                // 此处可添加--删除接口\n                // 删除成功调用fetchData方法更新列表\n                this.$message({\n                    type: 'success',\n                    message: '删除成功!'\n                })\n            }).catch(() => {\n                this.$message({\n                    type: 'info',\n                    message: '已取消删除'\n                })\n            })\n        },\n        // 批量删除\n        batchDelete() {\n            if (this.multipleSelection.length < 1) {\n                this.$message({\n                    message: '请先选择要删除的数据！',\n                    type: 'warning'\n                })\n            } else {\n                this.handleDelete()\n            }\n        },\n        // 获取数据列表\n        fetchData() {\n            this.listLoading = true\n            let url = \"http://localhost:8000/data/edit-words.json\"\n            // 获取数据列表接口\n            getTableList(this.listQuery, url).then(res => {\n                const data = res.data\n                if (data.code === 0) {\n                    this.total = data.data.total\n                    this.tableData = data.data.list\n                    this.listLoading = false\n                }\n            }).catch(() => {\n                this.listLoading = false\n            })\n        },\n        // 查询数据\n        onSubmit() {\n            this.listQuery.currentPage = 1\n            this.fetchData()\n        },\n        // 弹框\n        toDialog(component, row){\n            this.$refs[component].dialogVisible = true\n            this.$nextTick(()=>{\n                this.$refs[component].init(row)\n            })\n        },\n        //重置\n        resetForm() {\n            this.listQuery = this.initQuery();\n        },\n        initQuery() {\n            return {\n                uid: undefined,\n                app: undefined,\n                robotId: undefined,\n                currentPage: 1,\n                pageSize: 10\n            }\n        }\n    }\n}\n</script>\n\n<style lang=\"less\">\n.table-classic-wrapper {\n    .el-card {\n        min-height: 656px;\n    }\n\n    .control-btns {\n        margin-bottom: 20px;\n    }\n\n    .search-form {\n        padding-top: 18px;\n        margin-bottom: 15px;\n        background-color: #f7f8fb;\n    }\n\n    .el-table thead {\n        font-weight: 600;\n\n        th {\n            background-color: #f2f3f7;\n        }\n    }\n\n    .dialog-form {\n        .el-input {\n            width: 380px;\n        }\n\n        .footer-item {\n            margin-top: 50px;\n            text-align: right;\n        }\n    }\n\n    .upload-box,\n    .export-data {\n        width: 300px;\n        margin: 0 auto 30px;\n    }\n\n    .upload-box {\n        width: 156px;\n\n        .files-upload {\n            color: #20a0ff;\n        }\n    }\n\n    .hints {\n        font-size: 12px;\n        color: #aaa;\n        text-align: center;\n    }\n}\n</style>\n"],"sourceRoot":"src/views/manage/aia"}]}